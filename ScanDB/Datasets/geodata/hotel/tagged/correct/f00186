	    <script language="javascript">
		function displayTime(GMTDelta)
		{
			var now = new Date();
			var hour = now.getHours();
			var min = now.getMinutes();
			var secs = now.getSeconds();
			var offSet = now.getTimezoneOffset();
			
			var amPm = new String();
			//var GMTDelta = 0; //London is on GMT

			offSet = offSet / 60;
			//hour += (offSet + GMTDelta); //Now set to GMT time and Add in the GMT delta for this hotel
			
			
			var gmt = (hour + offSet);
			hour = gmt + GMTDelta;
			
			
			if (hour >= 24)
			{
			 hour -= 24
			}

			if (hour < 0)
			{
			 hour += 24
			}

			if (min < 10) min = '0' + min; //Implicitly convert to string and add a zero, if necessary
			if (secs < 10) secs = '0' + secs;
			//if (hour = '0') hour += '0';

			tStamp.innerText = hour + ':' + min; // + ':' + secs + amPm; //alter the value to display the current time
		}
</script>
