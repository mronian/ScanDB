<script language="javascript">
function emailLostPassword(chainId,hotelId,custEmail,langId)
{
	var paramString = "pHotelId=" + hotelId + "&pChainId=" + chainId + "&pcustEmail=" + custEmail + "&pLangId=" + langId;
	var windowURL = "/hotelsite/reservations/emailPassword.asp?" + paramString;
	var windowParams = 'height=350,width=400,scrollbars=no,resizable=yes'

	newWin = window.open(windowURL, "Retrieve_Password", windowParams);
	newWin.focus();
}
</script>

