// Site59's code
function wtl_updatePathCookie(pathcode) {
	document.cookie = "pathcode=" + pathcode;
}

function wtl_updateSearchCookie(searchcode) {
	if (searchcode != "") {
		document.cookie = "searchcode=" + searchcode;
	}
}

function wtl_updateMMICookie(mmicode) {
	if (mmicode != "") {
		document.cookie = "mmicode=" + mmicode;
	}
}

function wtl_updateHomepagePathCookie(pathcode) {
	var start = document.cookie.indexOf("pathcode=");
	if (start >= 0) {
		start += 9;
		var end = document.cookie.indexOf(";",start);
		if (end < 0) end = document.cookie.length;
		if (end - start < 100) {
			pathcode = document.cookie.substring(start,end) + pathcode;
		}
		else {
			pathcode = document.cookie.substring(start,end);
		}
	}
	document.cookie = "pathcode=" + pathcode;
}

// Flash detection script
var flash=new Object(); 

// Set some base values 
flash.installed=false; 
flash.version='0.0'; 

if (navigator.plugins && navigator.plugins.length) 
{ 
    for (x=0; x<navigator.plugins.length; x++) 
    { 
        if (navigator.plugins[x].name.indexOf('Shockwave Flash') != -1) 
        { 
            flash.version=navigator.plugins[x].description.split('Shockwave Flash ')[1]; 
            flash.installed=true; 
            break; 
        } 
    } 
} 
else if (window.ActiveXObject) 
{ 
    for (x=2; x<10; x++) 
    { 
        try 
        { 
            oFlash=eval("new ActiveXObject('ShockwaveFlash.ShockwaveFlash."+x+"');"); 
            if (oFlash) 
            { 
                flash.installed=true; 
                flash.version=x+'.0'; 
            } 
        } 
        catch(e) {} 
    } 
} 

if (flash.installed) {
	document.write('<meta name="DCSext.swf" content="' + parseInt(flash.version) + '">');
}

