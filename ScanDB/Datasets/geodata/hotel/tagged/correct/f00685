<script>
	var startDateField;
	var endDateField;
	var numNightsField;
	
	function setNumberOfNights()
	{
		var startDateText = startDateField.value.toLowerCase();
		var endDateText = endDateField.value.toUpperCase();
		
		if (startDateText == "mm/dd/yyyy" || endDateText == "mm/dd/yyyy")
		{
			return;
		}
		
		
		var startDate = getFieldDate(startDateText);
		var endDate = getFieldDate(endDateText);
		
		var diff = days_between(startDate, endDate);
		numNightsField.value = (isNaN(diff)) ? "" : diff;
	}
	
	function validateAvailabilityDates(sender, e)
	{
		var startDateText = startDateField.value.toLowerCase();
		var endDateText = endDateField.value.toLowerCase();
		
		var startDate = getFieldDate(startDateText);
		var endDate = getFieldDate(endDateText);
		
		if (startDateText == "mm/dd/yyyy" && endDateText == "mm/dd/yyyy")
		{
			e.IsValid = true;
			return;
		}
		
		if (startDateText == "mm/dd/yyyy" || endDateText == "mm/dd/yyyy")
		{
			e.IsValid = false;
			sender.innerText = "Please enter both a check in and check out date";
			return;
		}
		
		if (! isValidDate(startDateText) || ! isValidDate(endDateText) )
		{
			e.IsValid = false;
			sender.innerText = "Please enter a date in the following format MM/DD/YY";
			return;
		}
		
		//var today = new Date();
		//today = new Date(today.getFullYear(), today.getMonth(), today.getDate()); 
		//if (startDate.valueOf() == today.valueOf()) 
		//{
		//	e.IsValid = false;
		//	sender.innerText = "Weâ€™re sorry. We are unable to book rooms less than 24 hours in advance";
		//	return;
		//}
		
		e.IsValid = (endDate > startDate);
	}

</script>
