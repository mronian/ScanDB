<script language="JavaScript" type="text/javascript">

	function showRooms(i){
		if (i==1) {hide('room2Block'); hide('room3Block'); hide('room4Block'); hide('lblRoom1');}
		if (i==2) {show('room2Block'); hide('room3Block'); hide('room4Block'); show('lblRoom1');}
		if (i==3) {show('room2Block'); show('room3Block'); hide('room4Block'); show('lblRoom1');}
		if (i==4) {show('room2Block'); show('room3Block'); show('room4Block'); show('lblRoom1');}
	}
</script>
