<script language="JavaScript">

function initDate()
{
	
	var thetitle = new String(document.title);
	var thetitle2 = new Array();
	var now = new Date();
	var cityNameString = "";
	var mth = now.getMonth();
	var dd = now.getDate();
	var yy = now.getFullYear();
   var aryLimit = 0;
   
   thetitle2 = thetitle.split(" ");
   
   if (thetitle2[0] == "Cheap")
   {
   	for (j=0; j<thetitle2.length; j++)
   	{
   		thetitle2[j] = thetitle2[j+1];
   	}
   	 
   	aryLimit = thetitle2.length - 5;
   
   }
   else
   {
   	aryLimit = thetitle2.length - 5;
   }
   
  
   
   for (i=0; i<aryLimit; i++)
   {
   	if (i != (aryLimit - 1) && i != (aryLimit - 2))
   	{
   	
   		if (i == 0)
   		{
   			cityNameString = cityNameString + thetitle2[i];
   		}
   		else
   		{
   			cityNameString = cityNameString + " " + thetitle2[i];
   		}
   	}
   	
   }
   
   document.widgetForm.city.value = cityNameString;
   
	if (mth < 10)
	{
		mth == 0 + mth;
	}
	
	if (dd < 10)
	{
		dd == 0 + dd;
	}
  
	if ((mth==11) && (dd>=29)) 
	{
		document.widgetForm.arrivalDateMonth.selectedIndex = mth;
		document.widgetForm.arrivalDateDay.selectedIndex = dd - 1;
		document.widgetForm.arrivalDateYear.value= yy;
		document.widgetForm.departureDateMonth.selectedIndex = mth-mth;
		document.widgetForm.departureDateDay.selectedIndex = dd - dd;
		document.widgetForm.departureDateYear.value= yy+1;
	}
   else if ((dd>=29))
   {
		document.widgetForm.arrivalDateMonth.selectedIndex = mth+1;
		document.widgetForm.arrivalDateDay.selectedIndex = dd-dd;
		document.widgetForm.arrivalDateYear.value= yy;
		document.widgetForm.departureDateMonth.selectedIndex = mth + 1;
		document.widgetForm.departureDateDay.selectedIndex = (dd-dd)+1;
		document.widgetForm.departureDateYear.value= yy;
	}
	else
	{
		document.widgetForm.arrivalDateMonth.selectedIndex=mth;
		document.widgetForm.arrivalDateDay.selectedIndex=dd+1;
		document.widgetForm.arrivalDateYear.value= yy;
		document.widgetForm.departureDateMonth.selectedIndex=mth;
		document.widgetForm.departureDateDay.selectedIndex=dd+2;
		document.widgetForm.departureDateYear.value= yy;
	}
}

</script>

<!-- end new JS insertion -->




