<!-- the following code checks for version 6.+ of the Macromedia Flash player  -->
<!-- begin VBScript Flash Detect for IE/PC -->
<script language="vbscript">
	on error resume next
	var gotFlash
	gotFlash = False
	gotFlash = IsObject(CreateObject("ShockwaveFlash.ShockwaveFlash.6"))
</script>
<!-- end VBScript Flash Detect for IE/PC -->
<!-- begin javascript plugin detect functions -->
<script language="javascript">
var targetVersion = 6;
//Look for correct version of Flash plugin. If found, then launch window.
if ((navigator.plugins) && (navigator.plugins["Shockwave Flash"])) {
	var d = navigator.plugins["Shockwave Flash"].description;
	var v = parseInt(d.charAt(d.indexOf(".") - 1));
	//if the player is version 6 or above then set flag
	gotFlash = (v >= targetVersion)	? true : false;
}
</script>
<!-- end javascript plugin detect functions -->


