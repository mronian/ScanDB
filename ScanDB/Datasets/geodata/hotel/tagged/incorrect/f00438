	function popupReferPage(URL, l, t, w, h){
		var windowprops = "location=no,scrollbars=no,menubars=no,toolbars=no,resizable=no" + ",left=" + l + ",top=" + t + ",width=" + w + ",height=" + h;
		popup = window.open(URL,"MenuPopup",windowprops);
	}

	function mailPopup(URL, l, t, w, h){
		var windowprops = "location=no,scrollbars=no,menubars=no,toolbars=no,resizable=no" + ",left=" + l + ",top=" + t + ",width=" + w + ",height=" + h;
		popup = window.open(URL,"MenuPopup",windowprops);
	}

	function popupPage(URL, l, t, w, h){
		var windowprops = "location=no,scrollbars=no,menubars=no,toolbars=no,resizable=no" + ",left=" + l + ",top=" + t + ",width=" + w + ",height=" + h;
		popup = window.open(URL, "MenuPopup", windowprops);
	}

	function getWordCountInString(x){
		array1 = new Array();
		string1 = new String(x);
		array1 = string1.split(/\s+/);
		return array1.length;
	}

			
	function isBlank(szText){
		var i; 
		for(i=0; i<szText.length; i++){
			var c = szText.charAt(i);
			if (c!=' ' && c!='\t' && c!= '\r' && c!='\n'){
				return false;
			}
		}
		return true;
	}	

	function subWin(){ 
		win = window.open('/planning/journalGuideMatch.asp', "win", "toolbar=no,location=no,width=300,height=100,menubar=no,resizable=no,status=no,scrollbars=no");
	}
	
	function subWin2(){
		win = window.open('journalRatingBlurb.asp', "win", "toolbar=no,location=no,width=300,height=150,menubar=no,resizable=no,status=no,scrollbars=no");
	}
	
	function subWin4(){ 
		win = window.open('journalRatingGuidelines.asp', "win", "toolbar=no,location=no,width=600,height=150,menubar=no,resizable=no,status=no,scrollbars=no");
	}
	
