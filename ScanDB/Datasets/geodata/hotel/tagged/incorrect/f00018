<script>
  		mainTopPx = xHeight("main_top")
		
		function drawBot() {
            mainLeft = document.getElementById('main_left');
            mainRight = document.getElementById('main_right');
			
            // Use the baseline so we can recalculate
            mainLeftPx = originalLeftHeight
            mainRightPx = slideHeight
						
            // Determine where the footer starts and equalize the left and right well
            if (originalLeftHeight > slideHeight) {
                mainBotTop = mainLeftPx ;
                mainRight.style.height = mainLeftPx + "px";
                mainLeft.style.height = mainLeftPx + "px";
            } else {
                mainBotTop = slideHeight;
                mainRight.style.height = slideHeight + "px";
                mainLeft.style.height = slideHeight + "px";
            }

            // Home page has not left well so don't try to calculate it!
            if (urlpath != '/home') {
                // Expand the grey part of the left well to consume any extra space
                document.getElementById('left_well_bot').style.height = slideHeight - xHeight("left_well_promotions") - xHeight("left_well_features_cols") + "px";
            }
            
            // Position the footer and display it
            document.getElementById('main_bot').style.top = mainBotTop + mainTopPx + 1 + "px";
            document.getElementById('main_bot').style.visibility = 'visible';
		}
</script> 

<script>

// set div id's to variables

originalLeftHeight =  xHeight("main_left")

container1 = document.getElementById('cont1');
container2 = document.getElementById('cont2');
container3 = document.getElementById('cont3');
container4 = document.getElementById('cont4');
container5 = document.getElementById('cont5');
container6 = document.getElementById('cont6');

containerAd = document.getElementById('adCont');
containerAd2 = document.getElementById('adCont2');


content1 = document.getElementById('back1');
content2 = document.getElementById('back2');
content3 = document.getElementById('back3');
content4 = document.getElementById('back4');
content5 = document.getElementById('back5');
content6 = document.getElementById('back6');

if (navigator.appName.indexOf('Microsoft Internet Explorer') == -1)
{
tab1_arrow = document.getElementById('tab1_arrow');
tab2_arrow = document.getElementById('tab2_arrow');
tab3_arrow = document.getElementById('tab3_arrow');
tab4_arrow = document.getElementById('tab4_arrow');
tab5_arrow = document.getElementById('tab5_arrow');
tab6_arrow = document.getElementById('tab6_arrow');
}
		
//set initial values to open or close selected divs
if (urlpath == '/home') {
    content1.style.visibility = 'hidden'
    content2.style.visibility = 'visible'
    content3.style.visibility = 'hidden'
    content4.style.visibility = 'hidden'
    content5.style.visibility = 'hidden'
    content6.style.visibility = 'hidden'
 
    tab1_arrow.src = arrow_closed.src   
    tab5_arrow.src = arrow_closed.src
    tab6_arrow.src = arrow_closed.src
                
} else if (urlpath == '/hotelsandtravel') {
    content1.style.visibility = 'visible'
    content2.style.visibility = 'visible'
    content3.style.visibility = 'visible'
    content4.style.visibility = 'visible'
    content5.style.visibility = 'hidden'
    content6.style.visibility = 'hidden'

    //tab1_arrow.src = arrow_closed.src
    tab5_arrow.src = arrow_closed.src
    tab6_arrow.src = arrow_closed.src
} else {
    content1.style.visibility = 'hidden'
    content2.style.visibility = 'hidden'
    content3.style.visibility = 'visible'
    content4.style.visibility = 'visible'
    content5.style.visibility = 'hidden'
    content6.style.visibility = 'hidden'
    
		
    tab1_arrow.src = arrow_closed.src
    tab2_arrow.src = arrow_closed.src 
    tab5_arrow.src = arrow_closed.src
    tab6_arrow.src = arrow_closed.src
}


function slide(a){

// toggles tabs open/close

it = a;
switch(it){
	case 1 : 	
		if (content1.style.visibility == 'visible'){
		
		content1.style.visibility = 'hidden';
		tab1_arrow.src = arrow_closed.src
		
		
		}
		else{
		
		content1.style.visibility = 'visible';
		tab1_arrow.src = arrow_open.src
		}
	break
	case 2 : 	
		if (content2.style.visibility == 'visible'){
		
		content2.style.visibility = 'hidden';
		tab2_arrow.src = arrow_closed.src
		}
		else{
		
		content2.style.visibility = 'visible';
		tab2_arrow.src = arrow_open.src
		}
	break
	case 3 : 
		if (content3.style.visibility == 'visible'){
		
		content3.style.visibility = 'hidden';
		tab3_arrow.src = arrow_closed.src
		
		}
		else{
		
		content3.style.visibility = 'visible';
		tab3_arrow.src = arrow_open.src
		}
	break
	case 4 : 
		if (content4.style.visibility == 'visible'){
		
		content4.style.visibility = 'hidden';
		tab4_arrow.src = arrow_closed.src
		}
		else{
		
		content4.style.visibility = 'visible';
		tab4_arrow.src = arrow_open.src
		}
	break
		case 5 : 
		if (content5.style.visibility == 'visible'){
		
		content5.style.visibility = 'hidden';
		tab5_arrow.src = arrow_closed.src
		}
		else{
		
		content5.style.visibility = 'visible';
		tab5_arrow.src = arrow_open.src
		}
	break
		case 6 : 
		if (content6.style.visibility == 'visible'){
		
		content6.style.visibility = 'hidden';
		tab6_arrow.src = arrow_closed.src
		}
		else{
		
		content6.style.visibility = 'visible';
		tab6_arrow.src = arrow_open.src
		}
	break
	
	
}

if (content1.style.visibility == 'hidden') {	
content1_height_px = '0px'
}
else {
content1_height_px = content1.style.height
}


if (content2.style.visibility == 'hidden') {	
content2_height_px = '0px'
}
else {
content2_height_px = content2.style.height
}


if (content3.style.visibility == 'hidden') {	
content3_height_px = '0px'
}
else {
content3_height_px = content3.style.height
}


if (content4.style.visibility == 'hidden') {	
content4_height_px = '0px'
}
else {
content4_height_px = content4.style.height
}

if (content5.style.visibility == 'hidden') {	
content5_height_px = '0px'
}
else {
content5_height_px = content5.style.height
}

if (content6.style.visibility == 'hidden') {	
content6_height_px = '0px'
}
else {
content6_height_px = content6.style.height
}



content1_height = eval(content1_height_px.substring(0, content1_height_px.length - 2)) + 18;
content2_height = eval(content2_height_px.substring(0, content2_height_px.length - 2)) + 18;
content3_height = eval(content3_height_px.substring(0, content3_height_px.length - 2)) + 18;
content4_height = eval(content4_height_px.substring(0, content4_height_px.length - 2)) + 18;
content5_height = eval(content5_height_px.substring(0, content5_height_px.length - 2)) + 18;
content6_height = eval(content6_height_px.substring(0, content6_height_px.length - 2)) + 18;


// get natural heights of content cells

if (content1.style.visibility == 'visible'){
cont1_height = xHeight("back1") + 18;
}
else
{
cont1_height = 19;
}


	if (partnerOffersExists != true){
	cont2_height = 0;
	}
	else if (content2.style.visibility == 'visible'){
	cont2_height = xHeight("back2") + 18;
	}
	else
	{
	cont2_height = 19;
	}
	
	
	if (sponsoredPlacementExists != true){
	cont3_height = 0;
	}
	else if (content3.style.visibility == 'visible'){
	cont3_height = xHeight("back3") + 18;
	}
	else
	{
	cont3_height = 19;
	}



	if (travelPartnersExists != true){
	cont4_height = 0;
	}
	else if (content4.style.visibility == 'visible'){
	cont4_height = xHeight("back4") + 18;
	}
	else
	{
	cont4_height = 19;
	}


	if (featuredLinksExists != true){
	cont5_height = 0;
	}
	else if (content5.style.visibility == 'visible'){
	cont5_height = xHeight("back5") + 18;
	}
	else
	{
	cont5_height = 19;
	}

if (content6.style.visibility == 'visible'){
cont6_height = xHeight("back6") + 18;
}
else
{
cont6_height = 19;
}

//get natural height of ad cell and if nothing is in it, drop it to 0



contAd_height = xHeight("adCont");
contAd2_height = xHeight("adCont2");

//if (navigator.platform.indexOf('Mac') != -1)
//{contAd_height = 250}
//else if (contAd_height > 100) 
//{contAd_height = contAd_height;}
//else{contAd_height = 0;}



container1.style.top = 0 + "px"; 
container2.style.top = cont1_height + 0 + "px";

container3.style.top = cont2_height + cont1_height + 0 + "px";

container4.style.top = cont3_height + cont2_height + cont1_height + 0 + "px";

containerAd.style.top = cont4_height + cont3_height +cont2_height + cont1_height + 0 + "px";

container5.style.top = contAd_height + cont4_height + cont3_height + cont2_height + cont1_height + 0 + "px";
container6.style.top = contAd_height + cont5_height + cont4_height + cont3_height + cont2_height + cont1_height + 0 + "px";

containerAd2.style.top = cont1_height + cont2_height + cont3_height + cont4_height + cont5_height + cont6_height + contAd_height + 0 + "px";


slideHeight =  cont1_height + cont2_height + cont3_height + cont4_height + cont5_height + cont6_height + contAd_height + contAd2_height;

drawBot();

}



slide();

initPage = 1;



</script> 
