<script language="JavaScript" type="text/JavaScript">
<!--

function fitty() 
{
  var winWidth = 0, winHeight = 0;
  if( typeof( window.innerWidth ) == 'number' ) 
  {
    //Non-IE
    winWidth = window.innerWidth;
    winHeight = window.innerHeight;
  } 
  else 
  {
    if( document.documentElement && ( document.documentElement.clientWidth || document.documentElement.clientHeight ) ) {
      //IE 6+ in 'standards compliant mode'
      winWidth = document.documentElement.clientWidth;
      winHeight = document.documentElement.clientHeight;
    } else {
      if( document.body && ( document.body.clientWidth || document.body.clientHeight ) ) {
        //IE 4 compatible
        winWidth = document.body.clientWidth;
        winHeight = document.body.clientHeight;
      }
    }
  }
mainBox = document.getElementById('main');





	if (winWidth <= 840){
	mainBox.style.left = 0;
	}
	else{
	mainBox.style.left = ((winWidth / 2) - 420) + "px";
	}


}
//-->
</script>
<script>


function SetCookie(cookieName,cookieValue,nDays) {
 var today = new Date();
 var expire = new Date();
 if (nDays==null || nDays==0) nDays=1;
 expire.setTime(today.getTime() + 3600000*24*nDays);
   document.cookie = cookieName+"="+escape(cookieValue) + ";expires="+expire.toGMTString();
}

function ReadCookie(cookieName) {
 var theCookie=""+document.cookie;
 var ind=theCookie.indexOf(cookieName);
 if (ind==-1 || cookieName=="") return ""; 
 var ind1=theCookie.indexOf(';',ind);
 if (ind1==-1) ind1=theCookie.length; 
 return unescape(theCookie.substring(ind+cookieName.length+1,ind1));
}

function onClose(url, h, w)
{
    if (ReadCookie("closeAd") == "closeAd2" && window.screenTop > 10000)  //checks if closeAd cookie is set, and if it is, sets it to "closeAd21" and pops up a window
    {
      SetCookie("closeAd", "closeAd21");
	  
	 	var screenTop = (screen.height /2) - (h /2);
		var screenLeft = (screen.width /2) - (h /2);

		window.open(url, "closeAd", "height=" + h + ",width=" + w + ",top=" + screenTop + ",left=" + screenLeft + ",status=no,toolbar=no,menubar=no,location=no");
    } 
	 else if (ReadCookie("closeAd") == "closeAd2" && window.screenTop < 10000)  //checks if closeAd cookie is set, and if it is, sets it to "closeAd21" and pops up a window
    {
      SetCookie("closeAd", "closeAd2");
	  } 
	
    else if
      (ReadCookie("closeAd") == "closeAd21"){} // if window has already been popped up, does nothing
    else {
      SetCookie("closeAd", "closeAd2"); //if no cookie has been set, sets it to "closeAd2"
    }
}


</script>
<script>
   function addZero(vNumber){
       return ((vNumber < 10) ? "0" : "") + vNumber
   }

   vDate = new Date();

   var vDay              = addZero(vDate.getDate());
   var vMonth            = addZero(vDate.getMonth()+1);
   var vYearLong         = addZero(vDate.getFullYear());
   var vYearShort        = addZero(vDate.getFullYear().toString().substring(3,4));
   var vDateString       = "" + vMonth + "/" + vDay + "/" + vYearShort + "";
</script>
<script language="javascript">
var isFromGoogle;

function checkForIsFromGoogle(needle)
{
  var argv = new Array();
  var q = location.search;

  if (q.indexOf('campaignSource=G') != -1)
  {
    isFromGoogle = true;
  }
  else
  {
    isFromGoogle = false;
  }
}

checkForIsFromGoogle();
</script>
