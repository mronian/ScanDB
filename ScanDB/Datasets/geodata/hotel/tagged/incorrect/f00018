<SCRIPT LANGUAGE="JavaScript"><!--
// THE FOLLOWING FUNCTIONS ARE USED FOR THE NOBRAINER IMAGES

var splitArray = ["fake", "dummy", "data"];

function splits(string,text) {
// splits string at text
    var strLength = string.length, txtLength = text.length;
    if ((strLength == 0) || (txtLength == 0)) return;

    var i = string.indexOf(text);
    if ((!i) && (text != string.substring(0,txtLength))) return;
    if (i == -1) {
        splitArray[splitIndex++] = string;
        return;
    }

    splitArray[splitIndex++] = string.substring(0,i);
    
    if (i+txtLength < strLength)
        splits(string.substring(i+txtLength,strLength),text);

    return;
}

function split(string,text) {
    splitIndex = 0;
    splits(string,text);
}

function showArray() {
nbPic = splitArray[3];


if (nbPic == "nobraineritinerarygettycenter"){
nbPic = "the Getty"

}
else if (nbPic == "nobraineritinerarygraumanschinese"){
nbPic = "Grauman's"

}
else if(nbPic == "nobraineritinerarymusiccenter"){
nbPic = "the Music Center"

}

else if(nbPic == "nobraineritinerarystaplescenter"){
nbPic = "Staples"

}
else{
nbPic = ""
}
}
showArray();

//--></SCRIPT>

