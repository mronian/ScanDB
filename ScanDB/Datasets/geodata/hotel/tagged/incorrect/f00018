<script src="http://www.la.com/js/cookies.js"></script>
<script>

function startPage()
{
    q = location.search;

    getParam = function(arg) {
        if (q.indexOf(arg) >= 0) {
        var pntr = q.indexOf(arg) + arg.length + 1;
            if (q.indexOf("&", pntr) >= 0) {
                return q.substring(pntr, q.indexOf("&", pntr));
            } else {
                return q.substring(pntr, q.length);
            }
        } else {
            return null;
        }
    }


    var campaignIDValue =  getParam("campaignID");

    if(campaignIDValue != null){
        setCookie('campaignID',campaignIDValue);
    }
}

</script>
