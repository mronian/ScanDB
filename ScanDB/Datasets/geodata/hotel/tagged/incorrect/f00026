function submitAdvancedSearchForm() {
if (document.frm_Search.numberOfRooms.value == 5) {window.location="http://uk.hotels.com";
return false;
}
if ((document.frm_Search.city.value == null || document.frm_Search.city.value == "") &&
(document.frm_Search.otherDestText.value == "" || document.frm_Search.otherDestText.value == null)) {
alert("You must enter a city.".replace("'","\'"));
return false;
}
if (document.frm_Search.otherDestText.value != "" && document.frm_Search.otherDestText.value != null) {
document.frm_Search.city.value=document.frm_Search.otherDestText.value;
document.frm_Search.stateProvince.value='';
document.frm_Search.country.value='';
}
if (document.frm_Search.streetAddress != null &&
document.frm_Search.streetAddress.value.length > 0 &&
document.frm_Search.country.options[document.frm_Search.country.selectedIndex].value != "US") {
alert("A street address may only be specified for searches within the United States.".replace("'","\'"));
document.frm_Search.streetAddress.select();
document.frm_Search.streetAddress.focus();
return false;
}
return true; }


function MM_jumpMenu(targ,selObj,restore)
{ //v3.0
var CName = selObj.options[selObj.selectedIndex].text.toUpperCase();

if (selObj.selectedIndex > 0)
	{
		if (restore) selObj.selectedIndex=0;
		window.location.replace("Redirect.asp?Country=" + CName);
	}
else
	{
		return;
	}

}
	function bookmarksite(title, url)
	{
		if (document.all) 
		{
			window.external.AddFavorite(url, title);
		} 
		else if (window.sidebar) 
		{
			window.sidebar.addPanel(title, url, "");
		}
	}


</script>
