<SCRIPT LANGUAGE="JavaScript">
<!-- HIDE FROM BROWSERS

function newWindow(url,height,width)
{
   var new_width = 0;
   var new_heigh = 0;

   new_width = parseInt(width) + 100;
   new_height = parseInt(height) + 100;
   base = "http://www.res99.com/nexres/hotels/"
   urlWindow = window.open(base + url, 'Source', 'width=' + new_width + ',height=' + new_height + ',scrollbars=yes');
   urlWindow.focus;
}


// -->
</SCRIPT>


<script language="javascript">

var default_map_location = "/nexres/hotels/map_hotels.cgi?hid=11281656&map_only=yes&type=overview&width=510&length=500";

function requestMap(url) {
	var parentContainer = document.getElementById("hotelMap");
	if (!document.getElementById("mapFrame")){
		var mapIFrame = createIFrame("mapFrame");
		parentContainer.appendChild(mapIFrame);
		mapIFrame.src = url;
	}
	else{
		resizeIframe("mapFrame");
		}
}

function showhide(divName){
	var tempobj=document.all? document.all[divName] : document.getElementById(divName);
	var displayStyle = tempobj.style.display=='block' ? 'none' : 'block';
	tempobj.style.display = displayStyle;
}

var getFFVersion=navigator.userAgent.substring(navigator.userAgent.indexOf("Firefox")).split("/")[1];
var FFextraHeight=parseFloat(getFFVersion)>=0.1? 16 : 0; //extra height in px to add to iframe in FireFox 1.0+ browsers

function displayMap() {
//	var tempobj=document.all? document.all["mapFrame"] : document.getElementById("mapFrame");
	/*  Remove status div */
	var mapStatusDiv=document.all? document.all["mapStatus"] : document.getElementById("mapStatus");
	if (mapStatusDiv){
		var mapParentDiv = mapStatusDiv.parentNode;	
		mapParentDiv.removeChild(mapStatusDiv);
	}
	resizeIframe("mapFrame");
}

/*function createDiv(divName){
    var oDivElement = document.createElement("div"); 
    oDivElement.width="100%"; 
    oDivElement.height=300;
    oDivElement.name = divName;
    oDivElement.id = divName;
	return oDivElement;
}*/

function createIFrame(frmName) {
    var oIFrameElement = document.createElement("iframe"); 
    oIFrameElement.width="100%"; 
    oIFrameElement.height=0;
    oIFrameElement.frameBorder=0;
    oIFrameElement.name = frmName;
    oIFrameElement.id = frmName;
	oIFrameElement.scrolling = "no";
	return oIFrameElement;
}

function showhide(divName){
	var tempobj=document.all? document.all[divName] : document.getElementById(divName);
	var displayStyle = tempobj.style.display=='block' ? 'none' : 'block';
	tempobj.style.display = displayStyle;
}

//new overview javascript code

function initiate()
{
  var div = document.getElementById("tab0");
  showPane(div);
}

var currentPaneStyle = 0;
var currentTab = 0;

function tabstrip()
{
   this.tabs = new Array();
   this.add = addTab;
   this.write = writeTabstrip;
}

function tab(caption)
{
  this.setId = setId;
  this.caption = caption;
  this.write = writeTab;
}

function addTab(tab)
{
  tab.setId("tab" + this.tabs.length);
  this.tabs[this.tabs.length] = tab;
}

function setId(id)
{
  this.id = id;
}

/*function writeTab()
{
	document.write("<td style='padding: 1px 1px 0px 1px;'>");
	document.write("<div class='unselectedTab tabSize' id='" + this.id + "' onclick='showPane(this)'>");
	document.write("<table width='100%' height='100%' border='0' cellspacing='0' cellpadding='0'><tr>");
	document.write("<td class='leftTop'><img src='http://images.wctravel.com/images-general/powersearch/null.gif'></td>");
	document.write("<td class='tab'><img src='http://images.wctravel.com/images-general/powersearch/null.gif'></td>");
	document.write("<td class='rightTop'><img src='http://images.wctravel.com/images-general/powersearch/null.gif'></td></tr>");
	document.write("<tr><td rowspan='2' class='leftTabBorder'></td></tr>");
	document.write("<tr>");
	document.write("<td align='center' class='tabs'>" + this.caption + "</td>");
	document.write("<td class='rightTabBorder'><img src='http://images.wctravel.com/images-general/powersearch/null.gif'></td>");
	document.write("</tr></table>");
	document.write("</div>");
	document.write("</td>");
}*/

  // HERE IS WHERE THE TABS GET CALLED //
function writeTabstrip()
{
  document.write("<table border='0' cellspacing='0' cellpadding='0' align='left'><tr>");
  document.write("<TD><IMG height=1 src='http://images.wctravel.com/images-general/powersearch/null.gif' width=5 border=0></TD>");
  
  for(var i = 0; i < this.tabs.length; i++)
  {
    this.tabs[i].write();
  }
  document.write("</tr></table>");
}

function showPane(div)
{
  if(currentTab != 0)
  {
    //currentTab.style.backgroundColor = ""; //#5FB3F4 UNSELECTED COLOR OF TAB - after clicking on a tab, the other tabs default to this color
    //currentTab.style = document.getElementById.unselected.style;
	currentTab.className='unselectedTab tabSize';
  }
  //div.style = document.getElementById.mainTable.style;
  //div.style.backgroundColor = ""; //#9DD4FF - selected color of tab
  div.className = 'selectedTab tabSize';
  currentTab = div;

  if(currentPaneStyle != 0)
    currentPaneStyle.display = "none";
  var paneId = "pn_" + div.id;  
  var objPaneStyle = document.getElementById(paneId).style;
  objPaneStyle.display = "block";
  currentPaneStyle = objPaneStyle;
  if (div.id == 'tab1'){
  	requestMap(default_map_location);
  	resizeIframe("mapFrame");
  }
  //document.forms[0].tab.value = div.id;
  //HotelSearch.tab.value = div.id;
}
var travelReviewLoader	= new RemoteFileLoader('travelReviewLoader');
var nearbyHotelsLoader	= new RemoteFileLoader('nearbyHotelsLoader');

function fetch_hotels(selectionObj)
{
	var selectedOption = selectionObj.selectedIndex;
	var selection = selectionObj.options[selectedOption].value;
	var simUrl;
	if (selection.match("Distance"))
	{
		waitBox("nearby");
		var nearby_hotel = nearbyHotelsLoader.loadInto('/nexres/hotels/hotels_near.cgi?ses=b6b10587882acd3eb99bf08ca1cf14d054558ht&src=10006044&address_lat_lng=,&poi_name=&skip_hotels_id=11281656','nearby');
		simUrl = '/nexres/search/search_results.cgi?ses=b6b10587882acd3eb99bf08ca1cf14d054558ht&src=10006044&address_lat_lng=,&poi_name=&skip_hotels_id=11281656&avail=Y';
	}
	else if (selection.match("Rating"))
	{
		waitBox("nearby");
		var nearby_hotel = nearbyHotelsLoader.loadInto('/nexres/hotels/hotels_near.cgi?ses=b6b10587882acd3eb99bf08ca1cf14d054558ht&src=10006044&location_search_type=CITY&city=Seattle&state=WA&country=US&star_rating=1','nearby');	
		simUrl = '/nexres/search/search_results.cgi?ses=b6b10587882acd3eb99bf08ca1cf14d054558ht&src=10006044&location_search_type=CITY&city=Seattle&state=WA&country=US&star_rating=1&avail=Y';
	}
	// assign new url based on prev selection
	document.getElementById("MoreSim").href = simUrl;
}

function waitBox(div)
{	
	var divObj = document.all? document.all[div] : document.getElementById(div);
	divObj.innerHTML = "<TABLE width='100%' border=0 cellPadding=0 cellSpacing=0><tr class=mainTable><TD class=mainTable colspan='3'><IMG height=1 src='http://images.wctravel.com/images-general/powersearch/null.gif' width=1 border=0></TD></tr><TR><TD class=mainTable><IMG height=1 src='http://images.wctravel.com/images-general/powersearch/null.gif' width=1 border=0></TD><TD class=formfield style='PADDING-RIGHT: 10px; PADDING-LEFT: 10px; PADDING-BOTTOM: 5px; PADDING-TOP: 5px;' vAlign='middle' height='75' width='100%'><div class='loadingBox'><b>Please wait. Loading  ...</b> &nbsp;&nbsp;<img src='http://images.wctravel.com/images-general/powersearch/indicator.gif' /></div></TD><TD class=mainTable><IMG height=1 src='http://images.wctravel.com/images-general/powersearch/null.gif' width=1 border=0></TD></TR><!-- end review 1 --><TR class=mainTable><TD colSpan=3><IMG height=1 src='http://images.wctravel.com/images-general/powersearch/null.gif' width=1 border=0></TD></TR><TR><TD colSpan=3><IMG height=1 src='http://images.wctravel.com/images-general/powersearch/null.gif' width=1 border=0></TD></TR></table>"
}

window.onload = function(){	
	requestMap(default_map_location);
	
	
}
</script>
