<script language="JavaScript">
function distScript (v) {
if (v == "STYPE")
return document.distance.RR.selectedIndex == 0 ? "S" : "D";
else if (v == "R")
return document.distance.RR.selectedIndex == 0 ? "": "D";
}
function distLink () {
// Generate a distance sort link:
document.location.href='http://yellowpages.superpages.com/listings.jsp?SRC=portals&T=New+York+City&S=NY&PP=N&STYPE='+distScript('STYPE')+'&L=New+York+NY&CID=00000493329&IP=142.166.23.210&OO=1&RR='+document.distance.RR.value+'&R='+distScript('R')+'&CP=Travel+%26+Transportation%5EAccommodations%5EVacation+Home+Rental%5E';
}
</script>
