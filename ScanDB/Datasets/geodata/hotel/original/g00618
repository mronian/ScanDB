<script language="JavaScript">
var display = new Array (4);
display[0] = "location"; display[1] = "refine"; display[2] = "sort"; // display[3] = "showtimes";
function classChange (s) {
if (s != null) {
for (i=0; display[i]; i++) {
var o = document.getElementById(display[i]+"tab");
if (display[i] != s) {
o.className = "refinetabs";
} else {
o.className = "refinetabs_active";
for (j=0; display[j]; j++) {
if (display[j] != s)
hideform('do_'+display[j]);
else
showform('do_'+s);
}
}
}
setSPCookie("displayOptions",s,1);
}
}
eraseCookie("displayOptions");
</script>
