function OpenWindow( url, width, height ) {
  SmallWin = window.open(url, 'HostelPicture','scrollbars,resizable,height='+height+',width='+width);
  if (window.focus) {
    SmallWin.focus();
  }
  if (SmallWin.opener == null) {
    SmallWin.opener = window;
  }
}
function OpenWindow2( url, width, height ) {
  SmallWin = window.open(url, 'HostelPicture2','height='+height+',width='+width);
  if (window.focus) {
    SmallWin.focus();
  }
  if (SmallWin.opener == null) {
    SmallWin.opener = window;
  }
}
showHostelCountries('USA');
document.theForm.FABChoice.disabled = false;
document.theForm.NumNights.disabled = false;

populateFindABedPanel('USA' );
for ( i=0; i<document.theForm.FABChoice.length; i++ ) {
  if ( document.theForm.FABChoice.options[i].value == 'New York;USA' ) {
    document.theForm.FABChoice.selectedIndex = i;
  }
}


