<SCRIPT LANGUAGE="JavaScript">
function popup_pic(PicID){
	var calendarwindow;
	url = "pic_detail.cfm?PicID=" + PicID;
	calendarwindow = window.open(url,"thewindow","toolbar=no,location=0,directories=no,status=no,menubar=no,scrollbars=yes,resizeable=no,copyhistory=no,WIDTH=680,HEIGHT=520");
	calendarwindow.focus();
}
</SCRIPT>


