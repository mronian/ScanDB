<script language="javascript" type="text/javascript">
	function runSearch(){
		
			//form.act.value = 3;
			var regCity = document.form.region.value.split(/,/);
			var action = "./list.cfm?countryID="+regCity[0];
			//alert("reg:"+regCity[0]+" city:"+regCity[1]);
			if (regCity[1] != 0)//if cityID is populated
				action = action + "&cityID="+regCity[1];
			if (regCity[2] != 0)//if stateID is populated
				action = action + "&stateID="+regCity[2];
				
			document.form.action = action;
			document.form.submit();
	}
</script>

</td>
		<td valign="top">

