function popEmail()
{
  var lUrl = "/common/popup.jsp?title=Email+a+Friend&bodyUri=";
  lPageLinkUrl = encodeURI("http:%2F%2FRadisson.com%2Fhoteldirectory%2Fhotelbio.jsp%3Fkey%3Dhoteldirectory.info.%26hotelCode%3DCALAAIR");
  lUrl += encodeURI("/common/emailbody.jsp?pageLink=" + lPageLinkUrl);
  window.open(lUrl, 'email', 'dependent, scrollbars, toolbar=no, width=400,height=575').focus();  
}
</script>

