<!-- Entered /js/validation.jspf -->
<script type="text/javascript">









function ValidateCheckInCheckOutDates(pCheckInDay, pCheckInMonthYear, pCheckOutDay, pCheckOutMonthYear)
{
	var lCheckInDay = pCheckInDay;
	var lCheckInMonth = pCheckInMonthYear.substring(0,3);
	var lCheckInYear = pCheckInMonthYear.substring(4,8);

	var lCheckOutDay = pCheckOutDay;
	var lCheckOutMonth = pCheckOutMonthYear.substring(0,3);
	var lCheckOutYear = pCheckOutMonthYear.substring(4,8);

	var lCheckInMonthAsIntIndex = 0;
	if (lCheckInMonth == "Jan") lCheckInMonthAsIntIndex = 0;
	if (lCheckInMonth == "Feb") lCheckInMonthAsIntIndex = 1;
	if (lCheckInMonth == "Mar") lCheckInMonthAsIntIndex = 2;
	if (lCheckInMonth == "Apr") lCheckInMonthAsIntIndex = 3;
	if (lCheckInMonth == "May") lCheckInMonthAsIntIndex = 4;
	if (lCheckInMonth == "Jun") lCheckInMonthAsIntIndex = 5;
	if (lCheckInMonth == "Jul") lCheckInMonthAsIntIndex = 6;
	if (lCheckInMonth == "Aug") lCheckInMonthAsIntIndex = 7;
	if (lCheckInMonth == "Sep") lCheckInMonthAsIntIndex = 8;
	if (lCheckInMonth == "Oct") lCheckInMonthAsIntIndex = 9;
	if (lCheckInMonth == "Nov") lCheckInMonthAsIntIndex = 10;
	if (lCheckInMonth == "Dec") lCheckInMonthAsIntIndex = 11;

	var lCheckOutMonthAsIntIndex = 0;
	if (lCheckOutMonth == "Jan") lCheckOutMonthAsIntIndex = 0;
	if (lCheckOutMonth == "Feb") lCheckOutMonthAsIntIndex = 1;
	if (lCheckOutMonth == "Mar") lCheckOutMonthAsIntIndex = 2;
	if (lCheckOutMonth == "Apr") lCheckOutMonthAsIntIndex = 3;
	if (lCheckOutMonth == "May") lCheckOutMonthAsIntIndex = 4;
	if (lCheckOutMonth == "Jun") lCheckOutMonthAsIntIndex = 5;
	if (lCheckOutMonth == "Jul") lCheckOutMonthAsIntIndex = 6;
	if (lCheckOutMonth == "Aug") lCheckOutMonthAsIntIndex = 7;
	if (lCheckOutMonth == "Sep") lCheckOutMonthAsIntIndex = 8;
	if (lCheckOutMonth == "Oct") lCheckOutMonthAsIntIndex = 9;
	if (lCheckOutMonth == "Nov") lCheckOutMonthAsIntIndex = 10;
	if (lCheckOutMonth == "Dec") lCheckOutMonthAsIntIndex = 11;

	var lCurrentDate = new Date;

	// build javascript date object from params
	lCheckInDateObj = new Date(lCheckInYear, lCheckInMonthAsIntIndex, lCheckInDay, 23, 59, 59);  // set to midnight
	lCheckOutDateObj = new Date(lCheckOutYear, lCheckOutMonthAsIntIndex, lCheckOutDay, 23, 59, 59);

	// check in year leap?
	var lIsCheckInYearLeap = false;
	if ((lCheckInYear % 4)==0)
	{
		if ((lCheckInYear % 100)==0)
		{
			if ((lCheckInYear % 400)==0)
			{
				lIsCheckInYearLeap = true;
			}
		}
		else
		{
			lIsCheckInYearLeap = true;
		}
	}

	if (lCheckInMonth == "Feb")
	{
		if (lIsCheckInYearLeap)
		{
			if (lCheckInDay > 29)
			{
				alert("The date chosen for the Check-in appears to be invalid.\r\nPlease correct and resubmit.");
				return false;
			}
		}
		else
		{
			if (lCheckInDay > 28)
			{
				alert("The date chosen for the Check-in appears to be invalid.\r\nPlease correct and resubmit.");
				return false;
			}
		}
	}

	if (lCheckInMonth == "Sep" || lCheckInMonth == "Apr" || lCheckInMonth == "Jun" || lCheckInMonth == "Nov")
	{
		if (lCheckInDay > 30)
		{
			alert("The date chosen for the Check-in appears to be invalid.\r\nPlease correct and resubmit.");
			return false;
		}
	}
	else
	{
		// drop-down list has max 31 days
	}


	// check out year leap?
	var lIsCheckOutYearLeap = false;
	if (((lCheckOutYear % 4)==0))
	{
		if (((lCheckOutYear % 100)==0))
		{
			if (((lCheckOutYear % 400)==0))
			{
				lIsCheckOutYearLeap = true;
			}
		}
		else
		{
			lIsCheckOutYearLeap = true;
		}
	}

	if (lCheckOutMonth == "Feb")
	{
		if (lIsCheckInYearLeap)
		{
			if (lCheckOutDay > 29)
			{
				alert("The date chosen for the Check-out appears to be invalid.\r\nPlease correct and resubmit.");
				return false;
			}
		}
		else
		{
			if (lCheckOutDay > 28)
			{
				alert("The date chosen for the Check-out appears to be invalid.\r\nPlease correct and resubmit.");
				return false;
			}
		}
	}

	if (lCheckOutMonth == "Sep" || lCheckOutMonth == "Apr" || lCheckOutMonth == "Jun" || lCheckOutMonth == "Nov")
	{
		if (lCheckOutDay > 30)
		{
			alert("The date chosen for the Check-out appears to be invalid.\r\nPlease correct and resubmit.");
			return false;
		}
	}
	else
	{
		// drop-down list has max 31 days
	}

	// .getTime() returns milliseconds since 01/01/1970
	if ((lCheckInDateObj.getTime() < lCurrentDate.getTime()) || (lCheckOutDateObj.getTime() < lCurrentDate.getTime()))
	{
		alert("Please specify a date ON or AFTER todays date.");
		return false;
	}

	if (lCheckInDateObj.getTime() >= lCheckOutDateObj.getTime())
	{
		alert("Please specify a Check-out date AFTER the check-in date.");
		return false;
	}

	return true;

}































</script>
