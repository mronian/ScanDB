<script language="JavaScript" type="text/javascript">
function doGRPopLink(sLink) {
	var bReturn;
	if (bIsPopup == undefined) {
		bIsPopup = false;
	}
	sLink += "&isPopup=" + (bIsPopup ? "true" : "false");
	if (!bIsPopup) {
		var sWin = 'guestReviewsOrbitz'.substr(0,16);
		popUpGen(sLink,'480','600',sWin,false,"oGRPop");
	} else {
		document.location.href = sLink;
	}
	return false;
}
function doGRPopReview(s) {
	return doGRPopLink('/App/ViewGuestReviews?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec0&r=2&reviewSelectedIndex='+s);
}
function doGRPopReviewFromResults(s) {
	return doGRPopLink('/App/PrepareGuestReviews?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec1&r=3&hotelEntryKey='+s);
}
function doGRPopReviewAll() {
	return doGRPopLink('/App/ViewGuestReviews?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec2&r=4');
}
function doGRPopReviewSegment(s) {
	return doGRPopLink('/App/PerformHotelGuestReviewFilter?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec3&r=5&segmentType='+s);
}
function doGRPopReviewSort(s) {
	return doGRPopLink('/App/PerformHotelGuestReviewDetailSort?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec4&r=6&sorttype='+s);
}
function doGRPopExpert() {
	return doGRPopLink('/App/ViewExpertReview?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec5&r=7');
}
function doGRPopExpertFromResults(s) {
	return doGRPopLink('/App/PrepareExpertReview?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec6&r=8&hotelEntryKey='+s);
}
function doGRPopHint() {
	return doGRPopLink('/App/ViewHints?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec7&r=9');
}
function doGRPopHintSort(s) {
	return doGRPopLink('/App/PerformHotelGuestHintsSort?OSC=ELhP0SDIWW!806437857!183181347!7001!-1&z=dec8&r=a&sorttype='+s);
