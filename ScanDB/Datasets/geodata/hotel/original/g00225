	function imageOff(IMGName) {
		  document[IMGName].src = eval(IMGName + "off.src"); 
}
	function imageOn(IMGName) {
	
		  if (browser) { 
		  document[IMGName].src = eval(IMGName + "on.src");
		   }
}

function MM_findObj(n, d) { //v4.01
  var p,i,x;  if(!d) d=document; if((p=n.indexOf("?"))>0&&parent.frames.length) {
    d=parent.frames[n.substring(p+1)].document; n=n.substring(0,p);}
  if(!(x=d[n])&&d.all) x=d.all[n]; for (i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];
  for(i=0;!x&&d.layers&&i<d.layers.length;i++) x=MM_findObj(n,d.layers[i].document);
  if(!x && d.getElementById) x=d.getElementById(n); return x;
}

function MM_showHideLayers() { //v6.0
  var i,p,v,obj,args=MM_showHideLayers.arguments;
  for (i=0; i<(args.length-2); i+=3) if ((obj=MM_findObj(args[i]))!=null) { v=args[i+2];
    if (obj.style) { obj=obj.style; v=(v=='show')?'visible':(v=='hide')?'hidden':v; }
    obj.visibility=v; }
}


//SUBMENUS
//Global variables
var isNav, isIE, isNS6, isLoaded
isLoaded = false
if (document.all) {
  isIE = true;
  isNav = false;
  isNS6 = false;
}
else if (document.layers) {
  isNav = true;
  isIE = false;
  isNS6 = false;
}
else if (document.getElementById) {
  isNS6 = true;
  isIE = false;
  isNav = false;
}

function doNothing() {
  //Does Nothing
}


function getRealLeft() {
	xPos = this.offsetLeft;
	tempEl = this.offsetParent;
  	while (tempEl != null) {
  		xPos += tempEl.offsetLeft;
  		tempEl = tempEl.offsetParent;
  	}
	return xPos;
}

function getRealTop() {
	yPos = this.offsetTop;
	tempEl = this.offsetParent;
  	while (tempEl != null) {
  		yPos += tempEl.offsetTop;
  		tempEl = tempEl.offsetParent;
  	}
	return yPos;
}

function getImgX(whichImg){
	var theImg, theImgX
	theImg = eval("document." + whichImg);
	{theImgWidth = theImg.width}
	if (isIE){ theImg.getRealLeft = getRealLeft; }
	if (isNS6){ theImg.getRealLeft = getRealLeft; }
	if (isIE) {
	theImgX = theImg.getRealLeft() 
	}
	if (isNav) {
	theImgX = theImg.x
	}
	if (isNS6) {
	theImgX = theImg.getRealLeft()
	}
	return theImgX
}
function getImgY(whichImg){
	var theImg, theImgY
	theImg = eval("document." + whichImg);
	theImgHeight = theImg.height
	if (isIE){ theImg.getRealTop = getRealTop; }
	if (isNS6){ theImg.getRealTop = getRealTop; }
	if (isIE) {
	theImgY = theImg.getRealTop()
	}
	if (isNav) {
	theImgY = theImg.y
	}
	if (isNS6) {
	theImgY = theImg.getRealTop()
	}
	return theImgY
}

var overFront = false;
var nNumLayers = 

// Show SPECIAL's layers
function showFront(x,imgX, imgY)
{
	for (var i = 0; i <= nNumLayers; i ++) {
	  if (i != x) { hideAllFront(i);}
	}
	//alert (x);
	overFront = true;
	if (isNav) {
		theObject = eval("document.front" + x); 
		if (!theObject){
			setTimeout("hideAllFront(" + x + ");", 100);
		}
		else {
			theObject.left = imgX;
			theObject.top = imgY + 50;
			theObject.visibility = "visible";
		}
	}
	if (isIE) {
		theObject = eval("document.all.front" + x); 
		if (!theObject){
			setTimeout("hideAllFront(" + x + ");", 100);
		}
		else {
			theObject.style.left = imgX;
			theObject.style.top = imgY + 50;
			theObject.style.visibility = "visible";

		}
	}
	if (isNS6) {
		theObject = eval("document.getElementById('front" + x +"')"); 
		if (!theObject){
			setTimeout("hideAllFront(" + x + ");", 100);
		}
		else {
			theObject.style.left = imgX;
			theObject.style.top = imgY + 50;
			theObject.style.visibility = "visible";
		}
	}	
}


//Hide front Menu
function hideFront(x)
{
	overFront = false;
	if (isNav) {
  	y = setTimeout("hideAllFront(" + x + ")",2000);}
	if (isIE) {
  	y = setTimeout("hideAllFront(" + x + ")",100);}
	if (isNS6) {
  	y = setTimeout("hideAllFront(" + x + ")",300);}
}

function hideAllFront(x)
{
  //if (!overFront) {
	if (isNav) {
		theObject = eval("document.front" + x); 
		theObject.visibility = "hidden";
	}
	if (isIE) {
		theObject = eval("document.all.front" + x); 
		theObject.style.visibility = "hidden";
	}
	if (isNS6) {
		theObject = eval("document.getElementById('front" + x +"')"); 
		theObject.style.visibility = "hidden";
	}	
 // }
}


var numberMenus = 3; // the number of song-menus (zero based) 
var overMenus = false;

// Show song Menu
function showText(x,nTop)
{
	for (var i = 0; i <= numberMenus; i ++) {
	  if (i != x) { hideThem(i);	}
	}
	overMenus = true;
	
	if (isNav) {
		theObject = eval("document.sub" + x); 
		theObject.left = 129;
		theObject.top = nTop;
		theObject.visibility = "visible";
	}
	if (isIE) {
		theObject = eval("document.all.sub" + x); 
		theObject.style.left = 129;
		theObject.style.top = nTop;
		theObject.style.visibility = "visible";
	}
	if (isNS6) {
		theObject = eval("document.getElementById('sub" + x +"')"); 
		theObject.style.left = 129;
		theObject.style.top = nTop;
		theObject.style.visibility = "visible";
	}	
}


//Hide sub Menu
function hideText(x)
{
	overMenus = false;
	if (isNav) {
  	y = setTimeout("hideThem(" + x + ")",1000);}
	if (isIE) {
 	y = setTimeout("hideThem(" + x + ")",1000);}
 	if (isNS6) {
  	y = setTimeout("hideThem(" + x + ")",1000);}

}
function hideThem(x)
{
  if (!overMenus) {
	if (isNav) {
		theObject = eval("document.sub" + x); 
		theObject.visibility = "hidden";
	}
	if (isIE) {
		theObject = eval("document.all.sub" + x); 
		theObject.style.visibility = "hidden";
	}
	if (isNS6) {
		theObject = eval("document.getElementById('sub" + x +"')"); 
		theObject.style.visibility = "hidden";
	}	
  }
}



//-->
</script>
