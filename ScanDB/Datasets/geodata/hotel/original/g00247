<script type="text/javascript" language="javascript">
<!--
	// control prefix, given to JavaScript from code-behind
	var emailJobBoxPrefix = 'J_EmailJob' + '_';

	// flags controlling the state of the email box
	var emailJobOpenDID = '';
	var emailJobPending = false;

	// these will be set when the user opens the email job box for the first time
	var emailJobBox;
	var emailJobBoxInitialX;
	var emailJobBoxInitialY;

	// convenience function for grabbing an element
	function e(id){
		return (document.getElementById(emailJobBoxPrefix + id)) ?
			document.getElementById(emailJobBoxPrefix + id) : document.getElementById(id);
	}

	// display the email job box
	function openEmailJobBox(element, DID){
		if(!emailJobPending){
			if(!emailJobBox){
				// emailJobBox not set, now is the time to grab it
				emailJobBox = document.getElementById('EmailJobBox');
				emailJobBoxInitialX = findX(emailJobBox);
				emailJobBoxInitialY = findY(emailJobBox);
			}
			
			element.blur();
			
			if(DID == emailJobOpenDID){
				// if you reclick the same job e-mail link, have it close the box
				closeEmailJobBox(element);
			}
			else{
				emailJobOpenDID = DID;
				
				// initial display of form
				e('EmailJobBoxIntro').innerHTML = 'E-mail this job to a friend... Or yourself!';
				e('EmailJobBoxForm').style.display = '';
				e('JobDID').value = DID;

				// reposition box by the link that was clicked and make visibile
				emailJobBox.style.marginLeft = (findX(element) - emailJobBoxInitialX - 210) + 'px';
				emailJobBox.style.marginTop = (findY(element) - emailJobBoxInitialY + 16) + 'px';
				emailJobBox.className = 'ej_box';
			}			
		}
	}
	
	// close the email job box
	function closeEmailJobBox(element){
		if(emailJobBox){
			emailJobBox.className = 'ej_box ej_no_vis';
			emailJobOpenDID = '';
			element.blur();
		}
	}
	
	// submit to the AJAX page
	function submitEmailJobBox(){
		// display form in pending state
		emailJobPending = true;
		e('EmailJobBoxIntro').innerHTML = 'Sending E-mail...';
		e('EmailJobBoxClose').style.display = 'none';
		e('EmailJobBoxForm').style.display = 'none';
		
		var data = 'JobDID=' + e('JobDID').value + '&' +
				   'FromAddress=' + e('FromAddress').value + '&' +
				   'ToAddress=' + e('ToAddress').value + '&' +
				   'FromName=' + e('FromName').value + '&' +
				   'MessageBody=' + e('MessageBody').value;
				   
		submitCallback(data, 'EmailThisJob.aspx', handleEmailJobBoxResponse);
	}
	
	// handles the return signal from the AJAX page
	function handleEmailJobBoxResponse(success, responseText)
	{
		if(success)
		{
			switch(responseText)
			{
				case 'SUCCESS':
					e('EmailJobBoxIntro').innerHTML = 'This job was successfully e-mailed.';
					break;
					
				case 'INVALID':
					e('EmailJobBoxIntro').innerHTML = 'Error. Please check that the e-mails are valid.';
					e('EmailJobBoxForm').style.display = '';
					break;
					
				case 'FAILURE':
					e('EmailJobBoxIntro').innerHTML = 'An unknown error has occurred. Please try again later.';
					e('EmailJobBoxForm').style.display = '';
					break;
			}
		}
		else
		{
			// something went wrong with the AJAX callback
			e('EmailJobBoxIntro').innerHTML = 'An unknown error has occurred. Please try again later.';
			e('EmailJobBoxForm').style.display = '';
		}
		
		// in all cases, show close link again and leave pending state
		e('EmailJobBoxClose').style.display = '';
		emailJobPending = false;
	}	
	
	// called upon success
	function successEmailJobBox(){
		// indicate success state in intro text
		e('emailJobBoxIntro').innerHTML = 'This job was successfully e-mailed.';
	}

	// find X coordinate of an element
	function findX(element){
		var curleft = 0;
		if(element.offsetParent){
			while(element.offsetParent){
				curleft += element.offsetLeft
				element = element.offsetParent;
			}
		}
		else if(element.x){
			curleft += element.x;
		}
		return curleft;
	}

	// find Y coordinate of an element
	function findY(element){
		var curtop = 0;
		if(element.offsetParent){
			while(element.offsetParent){
				curtop += element.offsetTop
				element = element.offsetParent;
			}
		}
		else if(element.y){
			curtop += element.y;
		}
		return curtop;
	}

// -->
</script>

