<script language="javascript">
	function buildURL(keywords, sSC) {
		var fullURL;
		var queryString;
		var appendchar = '?';
		var ipath;
		
		
		if (baseURL.indexOf('?') != -1) {
			appendchar = '&';			
		}
		else {
			appendchar = '?';
		}
		
		fullURL = baseURL+appendchar+'MXJOBSRCHCRITERIA_Rawwords='+escape(keywords)+'&s_freeloc='+escape(Location)+'&MXJobSrchCriteria_JobTypes='+escape(Category)+'&ipath=SR&sc_cmp1='+sSC;
		window.location = fullURL;		
	}
</script>

