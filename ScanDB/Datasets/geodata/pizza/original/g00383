						function writeDogLover()
						{
							document.write("<TR><TD WIDTH=\"100%\"><CENTER><P><BR>");
							document.write("<br><A HREF=\"http://www.dogfriendly.com/servlet/refer?from=rightca&amp;dest=http://www.doglover.biz\" target=\"_blank\"><IMG SRC=\"http://www.dogfriendly.com/server/showcase/images/doglover.jpg\" ALIGN=\"BOTTOM\" BORDER=\"0\"></A><BR>");
							document.write("<A HREF=\"http://www.dogfriendly.com/servlet/refer?from=rightca&amp;dest=http://www.doglover.biz\" target=\"_blank\"></A>");
							document.write("<TABLE BORDER=\"0\" CELLPADDING=\"2\" CELLSPACING=\"4\" WIDTH=\"100%\">");
							document.write("<TR><TD WIDTH=\"100%\">");
							document.write("<FONT SIZE=\"3\"><p align=\"center\"><FONT SIZE=\"4\">Love dogs?</FONT><br>Like to meet others like you?<br>Join us at <a href=\"http://www.doglover.biz\"  target=\"_blank\">www.doglover.biz</a><br>Dating for people who love dogs.<br>As seen on BBC TV.<br>Free Premium Membership Until 2007.</p></font> ");
							document.write("</TD></TD></TABLE>");
