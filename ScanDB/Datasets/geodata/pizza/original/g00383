						function writeHI()
						{
							document.write("<TR><TD WIDTH=\"100%\"><CENTER><P><BR>");
							document.write("<A HREF=\"http://www.dogfriendly.com/servlet/refer?from=rightus&amp;dest=http://service.bfast.com/bfast/click?bfmid=37923149DFAMARKsiteid=40960402DFAMARKbfpage=hi_banner3DFAMARKbfxid=99507741\" target=\"_blank\"><IMG SRC=\"http://www.dogfriendly.com/server/headers/sponsors/hi_150x180.gif\" width=\"180\" height=\"150\" ALIGN=\"BOTTOM\" BORDER=\"0\"></A><BR>");
							document.write("<A HREF=\"http://www.dogfriendly.com/servlet/refer?from=rightus&amp;dest=http://service.bfast.com/bfast/click?bfmid=37923149DFAMARKsiteid=40960402DFAMARKbfpage=hi_banner3DFAMARKbfxid=99507741\" target=\"_blank\"><FONT SIZE=\"4\">Holiday Inn Has Many Dog-Friendly Locations listed here on DogFriendly.com</FONT></A><BR>");
							document.write("</CENTER></TD></TR>");
						}
								
						function writeCANDLEWOOD()
						{
							document.write("<TR><TD WIDTH=\"100%\"><CENTER><P><BR>");
							document.write("<A HREF=\"http://www.dogfriendly.com/servlet/refer?from=rightca&amp;dest=http://service.bfast.com/bfast/click?bfmid=37923149DFAMARKsiteid=40960402DFAMARKbfpage=1715290DFAMARKbfxid=99507763\" target=\"_blank\"><IMG SRC=\"http://images.bfast.com/clients/6c/120240cwpets.gif\" width=\"120\" height=\"240\" ALIGN=\"BOTTOM\" BORDER=\"0\"></A><BR>");
							document.write("<A HREF=\"http://www.dogfriendly.com/servlet/refer?from=rightca&amp;dest=http://service.bfast.com/bfast/click?bfmid=37923149DFAMARKsiteid=40960402DFAMARKbfpage=1715290DFAMARKbfxid=99507763\" target=\"_blank\"><FONT SIZE=\"4\">Save 5% when you book Pet - Friendly Candlewood Suites in advance</FONT></A><BR>");
							document.write("</CENTER></TD></TR>");
						}

						function writeDogFancy()
						{
							document.write("<TR><TD WIDTH=\"100%\"><CENTER><P>");
							document.write("<form method=\"get\" action=\"http://www.anrdoezrs.net/interactive\" target=\"_blank\" >");
							document.write("<img src=\"http://www.tqlkg.com/image-1382927-10273888\" height=\"1\" width=\"1\" border=\"0\">");
