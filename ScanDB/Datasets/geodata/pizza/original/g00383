						function writePetDate()
						{
							document.write("<TR><TD WIDTH=\"100%\"><CENTER><P><BR>");
							document.write("<br><A HREF=\"http://www.dogfriendly.com/servlet/refer?from=rightca&amp;dest=http://www.apetdate.com\" target=\"_blank\"><IMG SRC=\"http://www.dogfriendly.com/server/showcase/images/petdate.jpg\" ALIGN=\"BOTTOM\" BORDER=\"0\"></A><BR>");
							document.write("<A HREF=\"http://www.dogfriendly.com/servlet/refer?from=rightca&amp;dest=http://www.apetdate.com\" target=\"_blank\"><font size=\"4\">APetDate.com</font></A>");
							document.write("<TABLE BORDER=\"0\" CELLPADDING=\"2\" CELLSPACING=\"4\" WIDTH=\"100%\">");
							document.write("<TR><TD WIDTH=\"100%\">");
							document.write("<FONT SIZE=\"3\"><p align=\"center\">With our database and advanced easy searches you can locate the owner of the same breed pet in your local area for breeding or companionship.<br></p></font> ");
							document.write("</TD></TD></TABLE>");
							document.write("</CENTER></TD></TR>");
						}
						
						var rannum=new Number(Math.floor(160 * Math.random())+1); 
						
						if (rannum < 20)
						{							
							writeLQ();							
							writeACCOR();
						}
						if ((rannum >= 20) && (rannum < 40))
						{							
							writePetDate();
							writeBNUS();
						}
						if ((rannum >= 40) && (rannum < 50))
						{							
							writeSHOWCASE();
							writeCANDLEWOOD();
						}
						if ((rannum >= 50) && (rannum < 60))
						{							
							writeBNCA();
							writeSHOWCASE();
						}
						if ((rannum >= 60) && (rannum < 70))
						{							
							writeBordersUS();
							writeHI();
						}
						if ((rannum >= 70) && (rannum < 80))
						{							
							writeDogFancy();
							writeLQ();
						}
						if ((rannum >= 80) && (rannum < 100))
						{							
							writeWINERYBOOK();
							writeSHOWCASE();							
						}
						if ((rannum >= 100) && (rannum < 110))
						{							
							writeDogLover();
							writeLEGALBOOK();							
						}
						if ((rannum >= 110) && (rannum < 120))
						{							
							writeDogFancy();
							writeDogLover();							
						}
						if ((rannum >= 120) && (rannum < 130))
						{							
							writeHI();
							writeLEGALBOOK();							
						}
						if ((rannum >= 130) && (rannum < 140))
						{							
							writeWINERYBOOK();
							writeDFUS();							
						}
						if ((rannum >= 140) && (rannum < 200))
						{							
							writeDFCA();														
						}
