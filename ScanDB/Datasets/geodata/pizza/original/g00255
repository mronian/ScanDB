			if (fN == null || fN == "") {
				output = output + '<p id="login-welcome">[ Welcome ]</p>';
			} else {
				output = output + '<p id="login-welcome">[ Welcome, <a href="javascript:void(0)" onclick="window.open(\'http://my.espn.go.com/profile/user?username='+uN+'\',\'myProfile\',\'width=670,height=600\')">'+ fN + '</a>]</p>';
			}
			output = output + '</form>';
			return output;
		} else {
			var output = '<p id="login-nav"><strong id="signin">Sign In:</strong>  <a href="https://r.espn.go.com/espn/memberServices">Member Services</a> | <a href="http://r.espn.go.com/espn/memberservices/resetPassword">Forgot your password?</a> | <a style="font-weight: bold;" href="http://r.espn.go.com/espn/logout?appRedirect=' + encodeURIComponent(returnURL) + '">Sign Out</a> | <a style="font-weight: bold;" href="http://r.espn.go.com/espn/logout?appRedirect=' + encodeURIComponent(returnURL) + '">Not ' + fN + '?</a></p>';
			output = output + '<form name="login-form" id="login-form" method="post" action="https://r.espn.go.com/espn/insider/login">';
