	var returnURL = document.location.href;
	var xmlURL = 'http://' + document.location.hostname + '/espn/unilogin/loggedIn';
	var debug;
	var finalOutput;
	var appRedirect = 'null';

	if (appRedirect != '' && appRedirect != 'null')
	{
		returnURL = appRedirect;
	}

	if (returnURL.indexOf("univLogin02", 0) == -1)
	{
		if (returnURL.indexOf("?",0) == -1)
		{
			returnURL = returnURL + "?univLogin02=stateChanged";
		} else {
			returnURL = returnURL + "&univLogin02=stateChanged";
		}
	}

    function getCookie(name) {
        for (var i=0; i < cookies.length; i++) {
          cookie = cookies[i].split("=");
          if (cookie[0] == name)
            return cookie[1];
          }
          return null;
    }


	function getLoggedInMarkup(uN, fN, lG) {

        	if (lG == 1 || lG == "1")
        	{
			var output = '<p id="login-nav"><a href="https://r.espn.go.com/espn/memberServices">Member Services</a> | <a href="http://espn.go.com/sitetools/s/espnmsfaq.html">FAQ</a></p>';
			output = output + '<form action="http://r.espn.go.com/espn/logout" name="login-form" id="login-form">';
