<script type="text/JavaScript">

var currentRequest;
var nextRequest;

var URL = "/travel/xml/travel.xml";
var eventURL = "/travel/xml/travel/events";

var nextWeek = '20060807';
var lastWeek = '20060724';
var dma = '807';




var start;
var now;

function fetchDoc(date) {

	var ca = getElement("iframecal");
	ca.style.display = "none";		
	getElement("calendar").src = "/travel/util/calendar.html?date=" + date;
	setValue(getElement("showCal2"),"Show Calendar");
 	

	try {

		currentRequest = null;
		if (window.XMLHttpRequest)
		{
			currentRequest = new XMLHttpRequest();
			currentRequest.onreadystatechange = processStateChange;
			currentRequest.open("GET", URL + "?sDate=" + date + "&dma=" + dma, true);
			currentRequest.send(null);
		} else if (window.ActiveXObject) {
			currentRequest = new ActiveXObject("Microsoft.XMLHTTP");
			currentRequest.onreadystatechange = processStateChange;
			currentRequest.open("GET", URL + "?sDate=" + date + "&dma=" + dma, true);
			currentRequest.send();
		}

		nextRequest = null;
		if ( window.XMLHttpRequest)
		{
			nextRequest = new XMLHttpRequest();
			nextRequest.onreadystatechange = processEventChange;
			nextRequest.open("GET", eventURL + "?sDate=" + date + "&dma=" + dma, true);
			nextRequest.send(null);
		} else if (window.ActiveXObject) {
			nextRequest = new ActiveXObject("Microsoft.XMLHTTP");
			nextRequest.onreadystatechange = processEventChange;
			nextRequest.open("GET", eventURL + "?sDate=" + date + "&dma=" + dma, true);
			nextRequest.send();
		}

		
	} catch(e) { alert("error in fetch doc: " + e); }
}



function processStateChange() {
	if (currentRequest.readyState == 4) {	
		if (currentRequest.status == 200 && currentRequest.responseXML!=null) {
			try {				
				var city = currentRequest.responseXML.getElementsByTagName("city")[0];
				nextWeek = city.getAttribute("nextWeek");
				lastWeek = city.getAttribute("lastWeek");				
				for (var i=0; i<city.childNodes.length; i++){					
					if ( city.childNodes[i].nodeName == 'date' )
					handleDateString(city.childNodes[i].childNodes[0].nodeValue);
					if ( city.childNodes[i].nodeName == 'team' )
						handleTeamString(city.childNodes[i].childNodes[0].nodeValue, 
							city.childNodes[i].getAttribute("sportId"),
							city.childNodes[i].getAttribute("teamId"));		
				}				
			} catch(e) { alert("error in processSteateChange" + e); }		
		}
	}
}



function processEventChange() {	
	if (nextRequest.readyState == 4) {
		if (nextRequest.status == 200 && nextRequest.responseXML!=null) {
			try {
				var city = nextRequest.responseXML.getElementsByTagName("city")[0];
				if ( city.hasChildNodes ) {
					if ( city.childNodes.length == 0 ) {
						//CLEAR FOR GECKO
						var window=getElement("travel_events");		
						window.style.display="none";					
					} else {
						for (var i=0; i<city.childNodes.length; i++){
							if ( city.childNodes[i].nodeName == 'events' )
								handleEventString(city.childNodes[i].childNodes[0].nodeValue);	
						}
					}
				} else { 
					//CLEAR FOR IE
					var window=getElement("travel_events");		
					window.style.display="none";					
				} 
			} catch(e) { alert("error in processEventChange" + e.message); }		
		}
	}
}


function handleDateString (message) {
	var dates = message.split("|");
	var i=0; var length=dates.length;
	do {
		var parts = dates[i].split("~");
		if ( parts.length == 2 ) {
			setValue ( getElement("d"+i), 
				'<strong>' + parts[0] + '</strong><br />' + parts[1] );	
		}
		i++;
	} while (--length)
}

function handleEventString ( message ) {

	var events = message.split("|");

	var window=getElement("travel_events");	
	var style = window.style.display;
	var curElems=getElementsByClassName(window,"div","travel_blog");
	var curLines=getElementsByClassName(window,"div","travel_blog_rule_events");

	if ( events.length > 0 ) {		
		var i=0; var length=events.length;
		do {

			if (i < curElems.length){
				var parts = events[i].split("~");
				curElems[i].innerHTML="<strong>" + parts[0] + "</strong> " + parts[1];
			} else {
				var newContent = document.createElement('div');
				newContent.className = "travel_blog";
				
				var parts = events[i].split("~");
				newContent.innerHTML = "<strong>" + parts[0] + "</strong> " + parts[1];
				
				var newLine = document.createElement('div');
				newLine.innerHTML=" ";
				newLine.className="travel_blog_rule_events";
				
				window.appendChild(newContent);
				window.appendChild(newLine);
			}

			i++;
		} while(--length)

		if (events.length < curElems.length){
			for ( var j=events.length; j < curElems.length; j++){
				window.removeChild(curElems[j]);
				window.removeChild(curLines[j]);
			}
		}

		if ( style == "none" ) {
			window.style.display = "block";
			style = "block";
		}
	} 
}

function handleTeamString (message, sport, teamId) {
	try { 
		var games = message.split("|");
		var i=0; var length=games.length;

		do {
			var parts = games[i].split("~");
			if ( parts.length == 1 ) {			
				var window = getElement(sport + teamId + "_g" + i); 
				if ( window ) {
					setValue ( window, "&nbsp;" );
					window.style.backgroundColor = "#FFF";			
					window.title = "";
				}
			} else if (parts.length >= 3 ) {
				var window = getElement(sport + teamId + "_g" + i);
				
				if ( window ) {
					setValue ( window, parts[0] + "<br />" + parts[1] );
					var bgcolor = "#D6D6D4";
					if ( parts[3].indexOf('away') >= 0 ) {
						bgcolor = "#FFF";
					}
					window.style.backgroundColor = bgcolor;			
					window.title = parts[2];
				}
			}
			i++;
		} while (--length)
	} catch(e) { alert("error in handleTeamString " + e); }		
}



function showCalendar() {
	var window = getElement("iframecal");
	if ( window.style.display == "block" ) {
		window.style.display = "none";
		setValue(getElement("showCal2"),"Show Calendar");
	} else {
		window.style.display="block";
		setValue(getElement("showCal2"),"Hide Calendar");
	}
}

function getElement(elementid) {
	try {
		if (document.getElementById) {
			return document.getElementById(elementid);
		}
		if (document.all) {
			return document.all[elementid];
		}
	} catch(e) { alert(e); }
}

function setValue(domElem,message) {
	if(domElem)
	{
		message = message.replace(/#q#/g,'\'');
		if (domElem.innerHTML != message)
		{
			domElem.innerHTML = message;
		}
	}
}

function elapsedtime(start){
	var n = new Date();
	var s = n.getTime();
	return ( s - start );
}

function getElementsByClassName(oElm, strTagName, strClassName){
    var arrElements = (strTagName == "*" && document.all)? document.all : document.getElementsByTagName(strTagName);
    var arrReturnElements = new Array();
    strClassName = strClassName.replace(/\-/g, "\\-");
    var oRegExp = new RegExp("(^|\\s)" + strClassName + "(\\s|$)");
    var oElement;
    for(var i=0; i<arrElements.length; i++){
        oElement = arrElements[i];      
        if(oRegExp.test(oElement.className)){
            arrReturnElements.push(oElement);
        }   
    }
    return (arrReturnElements)
}

</script>





 <script language="JavaScript" type="text/javascript">
   function showtab(tab) {
    document.getElementById('overview').style.display = 'none';
    document.getElementById('eats').style.display = 'none';
    document.getElementById('hotels').style.display = 'none';
    document.getElementById(tab).style.display = 'block';
   }
 </script>


<script language="JavaScript" type="text/javascript">

var http = createRequestObject();

function createRequestObject() {
    var ro;
    var browser = navigator.appName;
    if(browser == "Microsoft Internet Explorer"){
        ro = new ActiveXObject("Microsoft.XMLHTTP");
    }else{
        ro = new XMLHttpRequest();
    }
    return ro;
}

function sndReq() {
    http.open('GET', '/travel/users/jwade/leadTest?c=6&DMA=' + DMA, true);
    http.onreadystatechange = handleResponse;
    http.send(null);  
}


function handleResponse() {

    if(http.readyState == 4){
    	try {
        var xml = http.responseText;
        document.getElementById('planner').innerHTML = xml;
    	} catch ( e ) { alert ( e.message ) ; }	    
    } 
    

}

</script>


