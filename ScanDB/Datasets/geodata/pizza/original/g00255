	function getLoginForm() {
		var output = '<p id="login-nav"><strong id="signin">Sign In:</strong> <a href="http://insider.espn.go.com/proxy/proxy.dll/insider/benefits?campaign=insider&source=univLogin">Try Insider</a> | <a href="https://r.espn.go.com/espn/memberServices">Member Services</a> | <a href="https://r.espn.go.com/espn/memberservices/register?registrationFormId=espn&sourceName=header">Register</a> | <a href="http://r.espn.go.com/espn/memberservices/resetPassword">Forgot your password?</a></p>';
        output = output + '<form name="login-form" id="login-form" method="post" action="https://r.espn.go.com/espn/insider/login">';
