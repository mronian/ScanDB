								<!-- navigators -->
								
	<script language="javascript">
		// these arrays are populated by each navigator rendered
		var navimages = new Array();
		var navlayers = new Array();
		
		var navsrcroot = 'http://img.icbdr.com/images/jobresults/nav';
		var activenavdisplayname;

		// values for the navigators
		var excrit = 'QID%3dA3849760841856%3bst%3da%3buse%3dALL%3bCTY%3dLos+Angeles%3bSID%3dCA%3bENR%3dNO%3bDTP%3dDR3%3bYDI%3dYES%3bIND%3dALL%3bPDQ%3dAll%3bJN%3dAll%3bPAYL%3d0%3bPAYH%3dgt120%3bPOY%3dNO%3bETD%3dALL%3bRE%3dALL%3bMGT%3dDC%3bSUP%3dDC%3bFRE%3d30%3bCHL%3dhp%3bQS%3dsid_unknown%3bSS%3dNO%3bTITL%3d0%3bJQT%3dRAD';
navimages['company'] = 'NCN_hiGate';
navlayers['company'] = 'NCN_pnlNavLayer';
img = new Image(); img.src = navsrcroot + 'company_on.gif';
navimages['city'] = 'NCT_hiGate';
navlayers['city'] = 'NCT_pnlNavLayer';
img = new Image(); img.src = navsrcroot + 'city_on.gif';
navimages['state'] = 'NST_hiGate';
navlayers['state'] = 'NST_pnlNavLayer';
img = new Image(); img.src = navsrcroot + 'state_on.gif';
navimages['pay'] = 'NPY_hiGate';
navlayers['pay'] = 'NPY_pnlNavLayer';
img = new Image(); img.src = navsrcroot + 'pay_on.gif';

		
		// navigator links call this to append excrit (if it hasn't already)
		function appendExCrit(link){
			if(link.href.indexOf('&excrit=') == -1){
				link.href += '&excrit=' + excrit;
			}
		}

		// Thanks to Peter-Paul Koch for these two functions! (http://www.quirksmode.org/)
		function findPosX(obj){
			var curleft = 0;
			if(obj.offsetParent){
				while(obj.offsetParent){
					curleft += obj.offsetLeft
					obj = obj.offsetParent;
				}
			}
			else if(obj.x){
				curleft += obj.x;
			}
			return curleft;
		}
		function findPosY(obj){
			var curtop = 0;
			if(obj.offsetParent){
				while(obj.offsetParent){
					curtop += obj.offsetTop
					obj = obj.offsetParent;
				}
			}
			else if(obj.y){
				curtop += obj.y;
			}
			return curtop;
		}

		// toggle a navigator on or off, also making sure all other navigators are off
		function toggleNav(togglenavdisplayname){
			var navlayer, gateimage;
			
			for(var i in navlayers){
				if(navlayers[i] != null){
					navlayer = document.getElementById(navlayers[i]);
					gateimage = document.getElementById(navimages[i]);
					
					if(i == togglenavdisplayname && i != activenavdisplayname){
						// this is the nav toggleNav() was called on and it is not active, so turn it on
						gateimage.src = navsrcroot + i + '_on.gif';
						navlayer.className = 'nav_div_on';
						navlayer.style.left = findPosX(gateimage) + 30 + 'px';
						navlayer.style.top = findPosY(gateimage) + 14 + 'px';
						activenavdisplayname = i;
					}
					else{
						// this is not the nav toggleNav() was called on or it is and it's on, so turn it off
						gateimage.src = navsrcroot + i + '_off.gif';
						navlayer.className = 'nav_div_off';
						if(i == togglenavdisplayname){
							// just turned off the active nav, so there is no active nav now
							activenavdisplayname = null;
						}
					}
				}
			}	
		}
				
	</script>


