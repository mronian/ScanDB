<!-- Begin
function bkmrk()
{
	if ((navigator.appName == "Microsoft Internet Explorer") && (parseInt(navigator.appVersion) >= 4) && (navigator.platform != "MacPPC")) {
		var url=window.location.href;
		var title=document.title;
		window.external.AddFavorite(url,title);
	}
	else {
		var msg = "Bookmark this page";
		if(navigator.platform == "MacPPC") msg += " by pressing APPLE-D";
		else if(navigator.appName == "Netscape") msg += " by pressing CTRL-D";
		else if(navigator.appName == "Opera") msg += " by pressing CTRL-T";
		alert(msg);
	}
}
function emailpage()
{
	var cleanurl = window.location.protocol +'//'+ window.location.host + window.location.pathname;
	var openurl='/email.html?url='+ cleanurl + '&title='+document.title;
	window.open(openurl, 'NAAemail','toolbar=1,width=500,height=450,status=1,scrollbars=1,resizable=1')
}

function remotelogin()
{
	window.location = 'https://www.nwuconnection.com/default.aspx?Page=BasicLogin&Username=' + window.document.login.Username.value + '&Password=' + window.document.login.Password.value;
}

// End -->
