function writeCSS(){
if(IE || NAV6){
document.write('<td width="86" height="23" style="position:relative;" align="center" id="DeptName" background="/2004_IMG/top_button.gif" onmouseover="ypSlideOutMenu.showMenu(\'dept\');this.style.backgroundImage=\'url(/2004_IMG/top_button_on.gif)\'" onmouseout="ypSlideOutMenu.hideMenu(\'dept\');this.style.backgroundImage=\'url(/2004_IMG/top_button.gif)\'"><a style="color:#666666;font-family:Arial;font-size:9pt;font-weight:bold;text-decoration:none;" href="/html/citizen/departments.htm">Departments</a></td>');
} else {
document.write('<td align="center" id="DeptName" background="/2004_IMG/top_button.gif"><a style="color:#666666;font-family:Arial;font-size:9pt;font-weight:bold;text-decoration:none;" href="/html/citizen/departments.htm">Departments</a></td>');
}
}
function testBrowser(){
  if(document.all){
  document.write('<input onclick="this.value=\'\'" style="font-size:9px;width:125px" value="Seattle.Gov" name="sp_q">&nbsp;<input style="font-size:9px" type="submit" value="Go">')
  }else{
  document.write('<input style="font-size:9px;" size="15" name="sp_q">&nbsp;<input style="font-size:9px;" type="submit" value="Go">')
  }
}

function writeCSS_NEW(){
if(IE || NAV6){
document.write('<td width="78" style="position:relative;" id="DeptName" onmouseover="ypSlideOutMenu.showMenu(\'dept\');" onmouseout="ypSlideOutMenu.hideMenu(\'dept\');"><a href="/html/citizen/departments.htm"><img src="/2004_img/btn_dept.gif" alt="City Departments" width="78" height="23" border="0"></a></td>');
} else {
document.write('<td width="78"><img src="/2004_img/btn_dept.gif" alt="City Departments" width="78" height="23" border="0"></td>');
}
}
function video(id){
loC = "http://www.seattlechannel.org/videos/video.asp?ID="+id
//alert (loC)
window.open(loC,"viewer","location=no,status=yes,scrollbars=no,resizable=no,width=675,height=472")
}

function videoContent(id){
loC = "http://www.seattlechannel.org/videos/video.asp?ID="+id+"&content=1"
window.open(loC,"viewer","location=no,status=yes,scrollbars=no,resizable=no,width=675,height=472")
}


