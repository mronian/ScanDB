<script text="text/javascript">

	function overtab(tab) {
		tab.style.background = "#ffffff";
		var img = tab.getElementsByTagName("IMG")[0];
		img.src = "/image/spacer.gif";
	}
	
	function outtab(tab) {
		tab.style.background = "#f1f1f1";
		var img = tab.getElementsByTagName("IMG")[0];
		img.src = "/image/tab_arrow.gif";
	}
</script>

