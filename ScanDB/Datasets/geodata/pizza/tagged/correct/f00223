function KW_submitOnce(obj){ // Copyright 2004 Kaosweaver
	d=document; if (d.getElementById) {for (i=0;i<obj.length;i++){var fObj=obj.elements[i];
	fObj.disabled=(fObj.type.toLowerCase()=="submit"||fObj.type.toLowerCase()=="reset")}}
}
 
 function xlaAFM_geturl(articleid,articletype){
	var newurl='http://sfsurvey.com/rd.asp?r=' + articletype + '=' + articleid;
	return newurl;
}

function sfsurvey_addtofavorites(articleid,articletype){
	window.external.addFavorite( xlaAFM_geturl(articleid,articletype), document.title );
}

function xlaAFM_printarticle(){
	window.print();
}

