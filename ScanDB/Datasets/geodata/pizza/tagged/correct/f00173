    function verifyRefineSearch()
{
  var selectedGroup;
  var selectedValue;
  var TrackingText;
  var group = document.EnhData.enhdatacats;
  var attr = document.EnhData.enhdatavalues;
  var placeholder = document.EnhData.placeholder;
  var rfc = document.EnhData.rfc;
  if (group.selectedIndex > 0 && attr.selectedIndex >= 0)
  {
    selectedGroup = group.options[group.selectedIndex].value;
    selectedValue = attr.options[attr.selectedIndex].value
    placeholder.value  = selectedValue;
    placeholder.name   = selectedGroup;
    rfc.value = group.selectedIndex + '|' + attr.selectedIndex + '|-1';
  }
  else if (group.selectedIndex != 0)
  {
   alert('Please choose the attribute to search on.');
   return false;
  }
  else
  {
    rfc.value = group.selectedIndex + '|-1|-1';
  }
  TrackingText = "By Category "
  TrackingText += selectedGroup;
  TrackingText = escape(TrackingText);
  _hbLink(TrackingText,'Narrow+Your+Search','');
  document.EnhData.submit();
  return true;
}
function verifyCats()
{
  var selectedCatUrl;
  var categories = document.CatList.similarcats;
  if (categories.selectedIndex >= 0)
  {
   selectedCatUrl = categories.options[categories.selectedIndex].value;
  }
  else
  {
   alert('Please choose a category.');
   return false;
  }
  TrackingText = "By Category "
  TrackingText += categories.options[categories.selectedIndex].text;
  TrackingText = escape(TrackingText);
  _hbLink(TrackingText,'Narrow+Your+Search','');
  var temp = gparam(selectedCatUrl, 'RFC');
  if (temp.length == 0)
  {
    location.href = selectedCatUrl+ '&RFC=-1|-1|' + categories.selectedIndex;
  }
  else
  {
    location.href = selectedCatUrl.replace('&RFC='+temp, '&RFC=-1|-1|'+categories.selectedIndex);
  }
  return false;
}
function gparam(searchfrom, searchfor)
{
 var tmp1=unescape(searchfrom.toLowerCase());
 var tmp2=searchfor.toLowerCase();
 var pos1=tmp1.indexOf('&' + tmp2 + '=');
 if(pos1==0)
 {
    var pos1=tmp1.indexOf('?' + tmp2 + '=');
 }
 var pos2=tmp1.indexOf("&",pos1+1);
 if(pos2<1) pos2 = tmp1.length+1;
 if(pos1>-1 && pos2>1)
 {
  pos1=pos1+searchfor.length+2;
  res=tmp1.substring(pos1,pos2);
 }
 else
 {
  res = ''; 
 }
 return res;
}
function DeselectList(CONTROL)
{
  for(var i = 0;i < CONTROL.length;i++)
  {
    CONTROL.options[i].selected = false;
  }
}
function ResetSelects()
{
  var temp = gparam(window.location.search.substring(1), 'RFC');
  var index_array;
  var group = document.EnhData.enhdatacats;
  var attr = document.EnhData.enhdatavalues;
  var categories = document.CatList.similarcats;
  DeselectList(group);
  DeselectList(attr);
  DeselectList(categories);
  if (temp.length > 0)
  {
    index_array = temp.split("|");
    for(var i = 0;i < index_array.length;i++)
    {
      var index = parseInt(index_array[i]);
      if (i==0 && index >= 0) {group.selectedIndex = index; group.onchange();}
      if (i==1 && index >= 0) {attr.selectedIndex = index; attr.onchange();}
      if (i==2 && index >= 0) categories.options[index].selected = true;
    }
  }
}

  //-->
