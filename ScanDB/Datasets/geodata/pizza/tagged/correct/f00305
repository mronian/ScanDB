				function changeImage(inImg) {
					if (inImg.src.indexOf("_Off") > 0) {
						inImg.src = inImg.src.replace(/_Off/, "_On");
					}
					else
						inImg.src = inImg.src.replace(/_On/, "_Off");
				}
			</script>
