function doLink(url)
{
var urlStr = url.split(",");
if (urlStr[(urlStr.length-1)] == "NEW")
{
urlStr.pop(); //remove the new window param from the url array
var newURL = urlStr.join(",");
window.open(newURL,"_blank");
}
else
location = url;
}
