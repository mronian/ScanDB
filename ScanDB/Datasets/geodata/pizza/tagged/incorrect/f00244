var URLPOS = 0, NAMEPOS = 1, PROPERTYPOS = 2;

function OpenWin(props)
{
	var lsProperties, myWin, lsURLPrefix;

	if ( props.split )
		lsProperties = props.split( "|" );
	else
		lsProperties = OldBrowserSplit( props, "|" );

	if ( lsProperties[URLPOS] != null )
	{
		if ( lsProperties[NAMEPOS] == null )
			lsProperties[NAMEPOS] = "";

		if ( lsProperties[PROPERTYPOS] == null )
			lsProperties[PROPERTYPOS] = "";

		lsURLPrefix = ""
		if ( lsProperties[URLPOS].substring( 0, 4 ) != "http" )
		{
			if (lsProperties[URLPOS].substring( 0, 1 ) != "/" )
				lsURLPrefix = "http://";
		}

		myWin = window.open( lsURLPrefix + lsProperties[URLPOS], lsProperties[NAMEPOS], lsProperties[PROPERTYPOS] );
	}
}

function OldBrowserSplit( OrigStr, tok )
	{
		var it = new Array();
		var counter = 0, cur = 0, prev = 0;
		var str = OrigStr;

		cur = str.indexOf( tok, prev );
		while ( cur != -1 )
		{
			it[counter] = str.substring( prev, cur );
			counter += 1;
			str = str.substring(cur+1);
			prev = 0;
			cur = str.indexOf( tok );
		}
		it[counter] = str.substring(prev);
		return it;
}


// check advanced search box
function doit()
{
	if ( document.SearchForm.qu.value.length < 4 )
	{
		alert("You must enter a phrase to search for with at least 4 characters!");
		return false;
	}
}



function refreshAds() {

var saf_iframe;

if (is_safari == true) {

	var frames;

	frames = document.getElementsByTagName("iframe");

	var adUBound;

	adUBound = frames.length;

	var adifrmcnt;

	adifrmcnt = adarray.length;


	for (var i = 0; i < adUBound; i++) {

		for (var n = 1; n < adifrmcnt; n++) {
			if (frames[i].id == adarray[n]) {

				frames[i].height = 0;

				frames[i].width = 0;

			}

		}

	}


}



else if ((bsr != "Netscape") || (ns6)) {

	if (ns6) {

		var ns6Iframe = document.getElementById("iframeads");	

		var refIframe = ns6Iframe.src;

	}

	else {

		var refIframe = document.all.iframeads.src;

	}

	var adIframe = window.frames["iframeads"]; 			

	var adUBound = adarray.length;


	for (i=1; i <=adUBound; i++) {

	var iframename = adarray[i];
	var adformname = "adform" + i;


		if (window.frames[iframename]) {

			if (ns6 && is_safari == false) {

			//alert(adIframe.document.forms[iframename].elements[0].value);

			//alert(window.frames[iframename].document.body.innerHTML);

			var bd = new String(adIframe.document.forms[iframename].elements[0].value);

			var brsrc = bd.search(/WN\snoad\S\SYes/gi);

			//alert(brsrc);

				if (brsrc == -1) {

				var wrapperid = "wrapper_" + iframename;
				var wrapper = document.getElementById(wrapperid);

					if (wrapper) {

					wrapper.style.display = "";

					}

					window.frames[iframename].document.open();

					window.frames[iframename].document.write(adIframe.document.forms[iframename].elements[0].value);

					window.frames[iframename].document.close();

				}

				else {

					var adiframe = document.getElementById(iframename);

					adiframe.height = 0;
					adiframe.width = 0;

				}

			}

			else {

			window.frames[iframename].document.body.innerHTML = adIframe.document.forms[iframename].elements[0].value;

			}

		}



	}



  

}


}

function checkad(p_adID) {
//alert(p_adID);

var wrapperid = "wrapper_" + p_adID; 
var wrapper = document.getElementById(wrapperid);

if (bsr == "Netscape" && parseInt(ver) >= 5) {

	var bd = new String(window.frames[p_adID].document.getElementById("AdHolder").innerHTML);

	var brsrc = bd.search(/WN\snoad\S\SYes/gi);

	var adiframe = document.getElementById(p_adID);

	if (brsrc != -1) {

		adiframe.height = 0;
		adiframe.width = 0;
	}
	else {

		if (wrapper) {
			//alert(wrapper);

			if (p_adID == "wnsz_42") {

				call_abpos('wnsz_42')

			}
			else {

				if (p_adID == "wnsz_41") {
				wrapper.style.width = "758px";
				}
				wrapper.style.display = "";

			}
		}



		adiframe.style.visibility = "visible";

	}

}
else if (bsr == "Netscape" && parseInt(ver) < 5 ) { } //Sniff out Netscape 4.x browser and not return anything
else {


	var iebd = new String(window.frames[p_adID].document.all.AdHolder.innerHTML);

	var iebdck = iebd.search(/WN\snoad.*Yes/g);

	//alert(iebdck);

	if ((iebdck != -1)||(iebd=="")) {

			document.all[p_adID].style.display = "none";

			//alert('closing down');

	}
	else {


		if (wrapper) {
			//alert(p_adID);

			if (p_adID == "wnsz_42") {

				call_abpos('wnsz_42')

			}
			else {

				wrapper.style.display = "";

			}
		}

		document.all[p_adID].style.visibility = "visible";



	}
}
}





var adcnt = 0;
var targetadtag = "Promotion 1";
var coreadcall = "/global/ad.asp?type=core&adtargwindow=Ad.TargetFrame.NewWin" + "&rnd=" + Math.round(Math.random() * 100000) + 1;
var adarray = new Array();

function coreAdsCreate (sz,owner, splitpct) {

adcnt++;

coreadcall += "&sz" + adcnt + "=" + sz  + "&cls" + adcnt + "=" + targetadtag + "&src" + adcnt + "=" + owner + "&frm" + adcnt + "=" + sz + "&spct" + adcnt + "=" + splitpct;

adarray[adcnt] = sz;

}



function wxSwap (id1, id2)	{

if (id2 == 'wximg') {

		if (ns6){

		ns6wxid1 = document.getElementById(id1);
		ns6wximg = document.getElementById(id2);

			if (ns6wxid1.style.display == 'none') {
				ns6wxid1.style.display = '';
				ns6wximg.src = '/images/static/gfx/wx_btn_on.gif';
			}
			else {
				ns6wxid1.style.display = 'none';
				ns6wximg.src = '/images/static/gfx/wx_btn_off.gif';
			}		
		}
		if (bsr == "Netscape" && ver < 5) { }
		else {


			if (document.all[id1].style.display == 'none') {
				document.all[id1].style.display = '';
				document.all[id2].src = '/images/static/gfx/wx_btn_on.gif';
			} 
			else {

				if (document.all[id1] && document.all[id2]) {

				document.all[id1].style.display = 'none';
				document.all[id2].src = '/images/static/gfx/wx_btn_off.gif';

				}
			}	   
		}	
} 
else {
		if (ns6){
			ns6id1 = document.getElementById(id1);
			ns6id1.style.display = 'none';

			ns6id2 = document.getElementById(id2);
			ns6id2.style.display = '';
		}
		if (bsr == "Netscape" && ver < 5) { }
		else {
			document.all[id1].style.display = 'none';	
			document.all[id2].style.display = '';
		}
}

}




function GetCookie (name)
{
if (document.cookie.length>0)
{
	var arg = name + "=";
	var alen = arg.length;
	var clen = document.cookie.length;
	var i = 0;

	while (i < clen)
	{
		var j = i + alen;

		if (document.cookie.substring(i, j) == arg) return getCookieVal (j);

		i = document.cookie.indexOf(" ", i) + 1;
		if (i == 0) break; 
	}
	return null;
}
}

// getCookieVal: function to return the decoded value of a cookie
function getCookieVal (offset)
{
var endstr = document.cookie.indexOf (";", offset);

if (endstr == -1) endstr = document.cookie.length;

return unescape(document.cookie.substring(offset, endstr));
}


function setCookie (name,value,expires,path,domain,secure)
{
if (document.cookie.length>0)
{
	document.cookie = name + "=" + escape (value) +
		((expires) ? "; expires=" + expires.toGMTString() : "") +
		((path) ? "; path=" + path : "") +
		((domain) ? "; domain=" + domain : "") +
		((secure) ? "; secure" : "");
}
}

function delCookie (name,path,domain) {
if (GetCookie(name)) {
alert("deleting cookie = " + GetCookie(name));
document.cookie = name + "=" +
((path == null) ? "" : "; path=" + path) +
((domain == null) ? "" : "; domain=" + domain) +
"; expires=Thu, 01-Jan-1970 00:00:01 GMT";
}
}

var multiple_entries_cookie_val;
var multiple_entries_update = false;
var multiple_entries = unescape(GetCookie("multipleentries"));

var multiple_entries_exp = new Date();
multiple_entries_exp.setDate("1");
multiple_entries_exp.setMonth("0");
multiple_entries_exp.setYear("2010");
multiple_entries_exp.setHours("0");
multiple_entries_exp.setMinutes("0");
var complete_cookie_delete = false;
var complete_cookie = GetCookie("completeditems");

var profile = GetCookie("FirstName");

if (profile != null) {
profile = profile.replace(/(\+)/g, " ");
}

else {

var profile_complete = GetCookie("EmailAddress");
if (profile_complete != null) {
	var profile_index = profile_complete.indexOf("@");
	profile = profile_complete.substring(0,profile_index);
}
}

