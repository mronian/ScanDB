function checkAllBoxes( strState, formID )
{
var objForm = eval ('document.' + formID);

if (strState == 'false')
{
	objForm.reset();

} else {
	for (i=0; i<objForm.length; i++)
	{
		if (objForm.elements[i].type == "checkbox" )
			objForm.elements[i].checked = true;
	}
}
}
