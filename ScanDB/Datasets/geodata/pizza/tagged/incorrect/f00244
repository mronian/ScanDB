function displayVideoHelp(p_os)
{
	self.name = "KRONCONTENT"; 
	
	if (p_os == 'mac-noPlugin') {
		var videoHelp = window.open('/global/Story.asp?s=2918340&displayHelp=true', 'CONTENT');
	} else {
		var videoHelp = window.open('/global/story.asp?s=1343812&displayHelp=true&displayHelp=true', 'CONTENT');
	}
	
	videoHelp.focus();
	window.name = "CONTENT";
}

