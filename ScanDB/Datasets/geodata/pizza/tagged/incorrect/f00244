function quickFormPost(frm,reqflds,customvals,dest) {

qform = eval('document.' + frm);

qform.SubmitBtn.disabled =  true;

document.body.style.cursor = 'wait';

var qfrmcnt = qform.elements.length



if (quickFormValidate(frm,reqflds) == true) {


	for (q=0; q<qfrmcnt; q++) {

		var qelem = qform.elements[q].name;

		var qelemval = qform.elements[q].value;


		if (qform.elements[q].type == "textarea") {

			var txtlen = qform.elements[q].value.length;

			var txtmxlen = qform.elements[q].getAttribute("maxlength");


			if (txtlen > txtmxlen) {

				qf_errmsg = "Text entered in this field is " + txtlen + " chars long. The maximum amount allowed is " + txtmxlen + "." ;

				alert(qf_errmsg);
				var textarea_alert = document.getElementById(qform.name + qelem);


				if (textarea_alert != null) {

					textarea_alert.color = "#CC0000";
				}
				return false;
			}
		}




		if (dest == '0') {

		var undscrint = qelem.indexOf('_');

			if (undscrint == 0) {

				qform.elements[q].name = qelem.substr(1);


			}

		}

		if (qform.submittedfields != null && qform.elements[q].type != "hidden" && qform.elements[q].type != "submit") {

			var prev_flds = qform.submittedfields.value.split("|");


			var prev_flds_len = prev_flds.length - 2;


			var prev_fld_str = prev_flds[prev_flds_len];


			if (qform.elements[q].name != prev_fld_str) {

			qform.submittedfields.value += qform.elements[q].name + "|";


			}
		}



	}	




	genformPost(qform, qfrmcnt, customvals);
	return false;

}
else {
	alert(qf_errmsg);
	qform.SubmitBtn.disabled = false;
	document.body.style.cursor = 'auto';
	return false;
}



}



function genformPost(qform, qfrmcnt, customvals) {

gen_frm = document.gen_form_post;


for (i=0; i < qfrmcnt; i++) {


var new_frm_fld = "";

if ((qform.elements[i].name == "LoginEmailAddress") || (qform.elements[i].type == "checkbox" && qform.elements[i].checked) || (qform.elements[i].type == "radio" && qform.elements[i].checked) || qform.elements[i].type == "select-one") {


	new_frm_fld = "<INPUT TYPE='HIDDEN' NAME='" + qform.elements[i].name + "' VALUE='" + qform.elements[i].value + "'/>";


}
else if (qform.elements[i].type != "checkbox" && qform.elements[i].type != "radio") {

	new_frm_fld = "<INPUT TYPE='HIDDEN' NAME='" + qform.elements[i].name + "' VALUE='" + escape(qform.elements[i].value) + "'/>";

}


gen_frm.innerHTML += new_frm_fld;

}


gen_frm.method = "POST";
gen_frm.action = customvals;
gen_frm.submit();




}



var qf_errmsg;
var err_flds = new Array();

function quickFormValidate(frm,reqflds) {

qf_errmsg = "";

if (err_flds) {

for (p=0; p < err_flds.length; p++) {

var err_alert = document.getElementById(frm + "_" + err_flds[p]);

if (err_alert != null) {

	err_alert.color = "#000000";
}

}


}

err_flds = new Array();

qform = eval('document.' + frm);

reqfldcnt = reqflds.length;

	for (k=0; k < reqfldcnt; k++) {

	var fld = eval('document.' + frm + '.' + reqflds[k][0]);


		if (reqflds[k].length <= 1) {


			if (fld != null) {

				var fldstr = fld.name;

				if (fldstr.indexOf('_') == 0) {

					fldstr = fld.name.substr(1);

				}					


				if (fld.value == '') {

					qf_errmsg = "Please fill in all required fields. Thank you.";

					if (bsr == "Microsoft Internet Explorer" && ver == 4) {

					err_flds[err_flds.length] = fldstr;


					}
					else {

					err_flds.push(fldstr);

					}
				}


				else if (fld.value != '' && fld.name == 'LoginEmailAddress') {

					var EmailPattern = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

					var email_input = qform.elements["LoginEmailAddress"].value;			

					var is_valid_email = email_input.match(EmailPattern);


					if (is_valid_email == null) {

						if (bsr == "Microsoft Internet Explorer" && ver == 4) {

						err_flds[err_flds.length] = "LoginEmailAddress";
						}
						else {
						err_flds.push("LoginEmailAddress");

						}
						qf_errmsg = "Please enter a valid Email Address";
						break;

					}

				}

				else if (fld.value != '' && fld.name == '_ZipCode') {

					var ZipCodePattern = /^\d{5}(-?\d{4})?$/;

					var zip_input = qform.elements["_ZipCode"].value;

					var is_valid_zip = zip_input.match(ZipCodePattern);

						if (is_valid_zip == null) {


							if (bsr == "Microsoft Internet Explorer" && ver == 4) {
							err_flds[err_flds.length] = "ZipCode";
							}
							else {
							err_flds.push("ZipCode");
							}
							qf_errmsg = "Please enter a valid Zip Code";
							break;

						}



				}
				else if (fld.value != '' && fld.type == "textarea") {

					var txtlen = fld.value.length;

					var txtmxlen = fld.getAttribute("MAXLENGTH");

					if (txtlen > txtmxlen) {

						if (bsr == "Microsoft Internet Explorer" && ver == 4) {
						err_flds[err_flds.length] = fldstr;
						}
						else {
						err_flds.push(fldstr);
						}
						qf_errmsg = "Text entered in this field is " + txtlen + " chars long. The maximum amount allowed is " + txtmxlen + "." ;
						break;
					}
				}

			}
		}


		else {

			reqchbxname = reqflds[k][0];

			if (reqchbxname.indexOf('_') == 0) {

			reqchbxname = reqflds[k][0].substr(1);

			}

			var checkbx_valid = false;

			var chkbxslen = reqflds[k].length - 1;

			for (m=1; m <= chkbxslen; m++) {	

				var fldchk = eval('document.' + frm + '.' + reqflds[k][m]);

				if ((fldchk.type == "checkbox" || fldchk.type == "radio") && (fldchk.checked)) {

					checkbx_valid = true;

				}
			}

			if (checkbx_valid == false) {

				if (bsr == "Microsoft Internet Explorer" && ver == 4) {
					err_flds[err_flds.length] = reqchbxname;
				}
				else {
					err_flds.push(reqchbxname);
				}

				qf_errmsg = "Please fill in all required fields. Thank you.";

			}

		}





}

var err_len = err_flds.length;

for (u=0; u < err_len; u++) {

var err_alert = document.getElementById(frm + "_" + err_flds[u]);

if (err_alert != null) {
	err_alert.color = "#CC0000";
}

}

if (err_len > 0) {

var is_valid = false;

}
else {

is_valid = true;

}

return is_valid;

}




function quickFormError() {

if (err_flds != null) {


for (p=0; p < err_len; p++) {

	var err_alert = eval("document.all." + frm + "_" + err_flds[p]);

	err_alert.color = "#000000";

}

}

err_flds = new Array();

var err_len = err_flds.length;

for (u=0; u < err_len; u++) {

	var err_alert = eval("document.all." + frm + "_" + err_flds[u]);

	err_alert.color = "#CC0000";

}



}





function quickSignupValidate(frm) {

qs_err_msg = "Please select an email";
is_valid = false;

qsform = eval('document.' + frm) ;

qsformcnt = qsform.elements.length;

for (v=0; v < qsformcnt; v++) {


if (qsform.elements[v].type == "checkbox" && qsform.elements[v].checked) {

	is_valid = true;
	break;
}

}


if (is_valid == false) {
alert(qs_err_msg);
}

return is_valid;

}

var theAnchor = "";
function ScrollFormTo(theAnchor) {

if (theAnchor != "") {


	var y = 0;

	var anchors = document.anchors;


	for (i=0; i < anchors.length; i++) {


			if (anchors[i].name == theAnchor) {


				if (bsr == "Netscape" && ver <= 5) {

					y = document.anchors[i].y;


				 }

				  else {

					  y = document.anchors[i].offsetTop;

				 }

			 window.scrollTo( 0, y );


			}

	}


}

}


var abpos_origHeight;
var abpos_loopcnt = 0;
var abpos_int;


function call_abpos(p_adID) {

//alert('call_abpos fired');

		if (abpos_origHeight != 0)
		{

			wn_adpos(p_adID);
			window.clearTimeout(abpos_int);

		}
		else
		{

			if (abpos_loopcnt <= 150)
			{

				abpos_int = window.setTimeout('call_abpos(p_adID);', 100);
				loopcnt++;

			}
			else
			{

				window.clearTimeout(abpos_int);
			}

		}



}



function wn_adpos (p_adID) {
var ad_px_left;
var ad_col4_left;

var tk_buffer;
var nv1_buffer;
var nv2_buffer;
var wx_buffer;
var ad_buffer;

 tk_buffer = 0;nv1_buffer = 21;nv2_buffer = 21;wx_buffer = 0; 

ad_buffer = tk_buffer + nv1_buffer + nv2_buffer + wx_buffer;


if (bsr == "Netscape" && ver < 5)

{

abpos_origHeight = window.innerHeight;
abpos_origWidth = window.innerWidth;

document.ad_text.top = document.ad_px.y - 10;
document.ad.top = document.ad_px.y + 2;

document.wrapper.height = abpos_origHeight;


}

else {



var wrapper = document.getElementById("wrapper_wnsz_42");

var ad_text = document.getElementById("ad_text_wnsz_42");

var ad_div = document.getElementById("ad_wnsz_42");

var ad_iframe = document.getElementById(p_adID);


if (bsr == "Netscape" && ver >= 5) {

	abpos_origHeight = document.bt_px.y;

 
	ad_left = document.col4_px.x + 135;
	 


	wrapper.style.height = abpos_origHeight + 80;
	wrapper.style.left = ad_left;

	if (document.wn_cat_hdr) {
		ad_text.style.top = document.wn_cat_hdr.y + document.wn_cat_hdr.height - 20;
		ad_div.style.top = document.wn_cat_hdr.y + document.wn_cat_hdr.height;
	}
	else {

		ad_text.style.top = document.banner_px.y - (ad_buffer + 20);
		ad_div.style.top = document.banner_px.y - (ad_buffer + 2);

	}

	ad_text.style.left = ad_left;
	ad_div.style.left = ad_left;


}
else {

	abpos_origHeight = document.body.scrollHeight;
	bt_px_bottom = document.all.bt_px.getBoundingClientRect().bottom;

 
	ad_left = document.all.col4_px.getBoundingClientRect().right + 5;
	 


	if (bt_px_bottom) {
	wrapper.style.height = bt_px_bottom + 5;
	}
	else{
	wrapper.style.height = abpos_origHeight;
	}

	wrapper.style.left = ad_left;

	if (document.all.wn_cat_hdr) {
		ad_text.style.top = document.all.wn_cat_hdr.getBoundingClientRect().bottom - 20;
		ad_div.style.top = document.all.wn_cat_hdr.getBoundingClientRect().bottom;
	}
	else {
		ad_text.style.top = document.all.banner_px.getBoundingClientRect().bottom - (ad_buffer + 20);
		ad_div.style.top = document.all.banner_px.getBoundingClientRect().bottom - ad_buffer;
	}

	ad_text.style.left = ad_left;
	ad_div.style.left = ad_left;



}


wrapper.style.visibility = "visible";

ad_text.style.visibility = "visible";

ad_div.style.visibility = "visible";


}


}

function getLEDate(value) {
	var MONTHS = ['Jan', 'Feb', 'March', 'April', 'May', 'June', 'July', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
	var JANUARY = 0, JULY = 6, YEAR = 2006, DAY = 1, HOUR_MILLIS = 3600000;
	var dateObj = new Date(value);
	var year = dateObj.getFullYear();
	var hour = dateObj.getHours();
	var dlsOffset = (new Date(YEAR, JULY, DAY)).getTimezoneOffset();
	var observesDLS = dlsOffset != (new Date(YEAR, JANUARY, DAY)).getTimezoneOffset();
	if ((observesDLS && dateObj.getTimezoneOffset() == dlsOffset) || (!observesDLS && wnDateInDaylightSavings(dateObj))) {
		if (!hour) {
			dateObj = new Date(dateObj.getTime() - HOUR_MILLIS);
			hour = dateObj.getHours();
		}
		else {
			hour = hour - 1;
		}
	}
	var minute = dateObj.getMinutes();
	var ap = 'AM';
	if (hour > 11) {
		ap = 'PM';
	}
	if (hour > 12) {
		hour = hour - 12;
	}
	if (hour == 0) {
		hour = 12;
	}
	if (hour < 10) {
		hour = '0' + hour;
	}
	if (minute < 10) {
		minute = '0' + minute;
	}
	var timezone = '';
	try {
		var tzMatch = dateObj.toString().match(new RegExp('(\\w+(D|S)T)\\s+' + year));
		if (tzMatch && tzMatch.length > 1) {
			timezone = tzMatch[1];
		}
	}
	catch (e) {
		// pass
	}
	return MONTHS[dateObj.getMonth()] + ' ' + dateObj.getDate() + ', ' + year + ' ' + hour + ':' + minute + ' ' + ap + ' ' + timezone;
}

function wnDateInDaylightSavings (dateObj) {
	var year = dateObj.getFullYear();
	var isNewDLSDates = year > 2006;
	var START_MONTH = (isNewDLSDates) ? 2 : 3;
	var END_MONTH = (isNewDLSDates) ? 10 : 9;
	var month = dateObj.getMonth();
	if (month < START_MONTH || month > END_MONTH) {
		return false;
	}
	var START_DAY = 1, SUNDAY = 0, HOUR = 2, DOW = 7, DAY_MILLIS = 86400000, EDT_OFFSET = -300, MINUTE_MILLIS = 60000;
	var tzOffset = (-dateObj.getTimezoneOffset() - EDT_OFFSET) * MINUTE_MILLIS;
	// calculate start of daylight saving time
	var startDLS = new Date(year, START_MONTH, START_DAY, HOUR);
	var startDay = startDLS.getDay();
	if (startDay != SUNDAY) {
		startDLS = new Date(startDLS.getTime() + ((DOW - startDay) * DAY_MILLIS));
	}
	if (isNewDLSDates) {
		startDLS = new Date(startDLS.getTime() + (DOW * DAY_MILLIS));
	}
	startDLS = new Date(startDLS.getTime() + tzOffset);
	// calculate end of daylight saving time
	var END_DAY = (isNewDLSDates) ? 1 : 31;
	var endDLS = new Date(year, END_MONTH, END_DAY, HOUR);
	var endDay = endDLS.getDay();
	if (endDay != SUNDAY) {
		var offset = (isNewDLSDates) ? DOW - endDay : -endDay;
		endDLS = new Date(endDLS.getTime() + (offset * DAY_MILLIS));
	}
	endDLS = new Date(endDLS.getTime() + tzOffset);
	return dateObj >= startDLS && dateObj < endDLS;
}


