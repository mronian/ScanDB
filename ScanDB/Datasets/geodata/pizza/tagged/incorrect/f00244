		var currenturl_query = document.location.search;

		var currenturl_pairs1 = currenturl_query.split("?");


		if (currenturl_pairs1 != "") {

			var currenturl_pairs = currenturl_pairs1[1].split("&");

			for(var i = 0; i < currenturl_pairs.length; i++) {
				var currenturl_pos = currenturl_pairs[i].indexOf('=');
				if (currenturl_pos == -1) continue;
				var currenturl_argname = currenturl_pairs[i].substring(0,currenturl_pos);
				var currenturl_value = currenturl_pairs[i].substring(currenturl_pos+1);
				currenturl_args[currenturl_argname.toLowerCase()] = unescape(currenturl_value);
			}


			var currenturl_pagetype;
			var currenturl_nav;

			if (currenturl_args["c"] != null || currenturl_args["C"] != null) {

				currenturl_pagetype = "?c=" + currenturl_args["c"];

			}
			else if (currenturl_args["s"] != null || currenturl_args["S"] != null) {

				currenturl_pagetype = "?s=" + currenturl_args["s"];

			}
			else if (currenturl_args["l"] != null || currenturl_args["L"] != null) {

				currenturl_pagetype = "?l=" + currenturl_args["l"];

			}
			else {

				currenturl_pagetype = "";

			}

			if (currenturl_args["nav"]) {

				currenturl_nav =  "&nav=" + currenturl_args["nav"];

			}
			else {

				currenturl_nav = "";
			}

		}

		if (currenturl_pagetype == "?s=8945") {

			currenturl = "http://" + document.location.hostname;

		}

		else {

			currenturl = "http://" + document.location.hostname + document.location.pathname + currenturl_pagetype + currenturl_nav;

		}


	

		function mOvr(src,clrOver,aclrOver) {


			if ((src.className == "l1off") || (src.className == "l1on")) {

				src.style.cursor = 'hand';

				src.style.backgroundColor = clrOver;

				src.children.tags('A')[0].style.color = aclrOver;

				src.children.tags('A')[0].style.textDecoration = 'underline';


			}

			else {

					if (ns6) {

					var ns6src1 = src + ":" + 1;
					var ns6src2 = src + ":" + 2; 
					var ns6src3 = src + ":" + 3;

					var ns6navbtn1 = document.getElementById(ns6src1);				
					ns6navbtn1.style.backgroundColor = clrOver; 

					var ns6navbtn2 = document.getElementById(ns6src2);				
					ns6navbtn2.style.backgroundColor = clrOver; 
					var ns6navbtn2txt = ns6navbtn2.getElementsByTagName("A");
					ns6navbtn2txt[0].style.color = aclrOver;
					ns6navbtn2txt[0].style.textDecoration = 'underline';

					var ns6navbtn3 = document.getElementById(ns6src3);				
					ns6navbtn3.style.backgroundColor = clrOver; 

					}
					else {

					var newsrc
					var i;
					for (i=1; i <= 3; i++) {

						newsrc = src + ":" + i;

						document.all[newsrc].style.cursor = 'hand';

						document.all[newsrc].style.backgroundColor = clrOver;


						if (document.all[newsrc].children.tags('A').length != 0) {
							document.all[newsrc].children.tags('A')[0].style.color = aclrOver;
							document.all[newsrc].children.tags('A')[0].style.textDecoration = 'underline';
						}
					}

				}

			}
		}



