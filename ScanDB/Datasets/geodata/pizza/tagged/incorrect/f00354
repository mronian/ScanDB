function doPopup() {
   if(document.cookie.indexOf("POPUP") == -1)
      window.open("http://www.longisland.com/popup_banner.php", "", "width=200,height=320,toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=0,resizable=0");
}

function handleSearch(form) {
     if (form.scope.value=='') {
        alert('Please enter query.');
        form.scope.focus();
        return false;
     }
     form.scope.value = form.scope.value.toLowerCase();
     return true;
}

function getCategory(category) {
     document.location = "http://www.longisland.com/lisearch.php?scope="+escape(category)+"&ui=sr";
}

