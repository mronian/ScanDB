//function to activate images

function imgOn(imgName) {
        if (document.images) {
                document[imgName].src = eval(imgName + ".src");
        }
}

//function to activate images

function imgOn(imgName) {
        if (document.images) {
                document[imgName].src = eval(imgName + ".src");
        }
}

//functions to deactivate images

function imgOff(imgName) {
        if (document.images) {
                document[imgName].src = "moi.gif";
        }
}

function navOff(navName) {
        if (document.images) {
                document[navName].src = eval(navName + "off.src");
        }
}

//Set the intial focus on the userid field
function setInputFocus() {   if (document.LoginForm.Login.value == "")  document.LoginForm.Login.focus();  }

//Depending on where the user is, validate
//data and either set focus on the next field
//or submit the form
function StartApp(i)
{
    if (document.LoginForm.Login.value == "")
    {
        alert("Please enter an LOGIN and PASSWORD to continue!");
        document.LoginForm.Login.focus();
        return false;
    }
    if (i == 1)
    {
        document.PasswordForm.Password.focus();
        return false;
    }
    else
    {
        if (document.PasswordForm.Password.value == "")
        {
            alert("Please enter an LOGIN and PASSWORD to continue!");
            document.PasswordForm.Password.focus();
            return false;
        }
    }

    //frmPoster is used as a work area to populate 
    //and then submit the form 
    document.continueForm.Login.value               = document.LoginForm.Login.value;
    document.continueForm.Password.value  = document.PasswordForm.Password.value; 
    document.continueForm.submit();
    return false;
}

