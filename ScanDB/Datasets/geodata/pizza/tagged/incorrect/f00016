                function clearKeyword()
                {
                    var defaultKeyword = 'Enter Name, Cuisine, Detail, etc.';

                    if (document.searchBox.keywordSearch.value == defaultKeyword)
                    {
                       document.searchBox.keywordSearch.value = '';
                    }
                }

                function verifyKeyword(onBlur)
                {
                    var defaultKeyword = 'Enter Name, Cuisine, Detail, etc.';

                    if (onBlur)
                    {
                        if (document.searchBox.keywordSearch.value == defaultKeyword)
                        {
                           document.searchBox.keywordSearch.value = '';
                        }
                    }
                }            
