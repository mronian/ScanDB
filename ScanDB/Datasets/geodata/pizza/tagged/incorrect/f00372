<script>
	function validateSearchForm()
	{
	
		var msanumberList = document.searchForm.msanumber == null ? document.all("msanumber") : document.searchForm.msanumber
		var cityObj = document.searchForm.City == null ? document.all("City") : document.searchForm.City
		var stateObj = document.searchForm.State == null ? document.all("State") : document.searchForm.State
		var zipObj = document.searchForm.Zip == null ? document.all("Zip") : document.searchForm.Zip
		var areacodeObj = document.searchForm.areacode == null ? document.all("areacode") : document.searchForm.areacode
		
		if (msanumberList.selectedIndex == 0 && cityObj.value == ""  && stateObj.selectedIndex == 0 && zipObj.value == "" && areacodeObj.value == "" )
		{
			alert("Please specify a Metro Area, City and State, Zipcode, or Area Code ")
			msanumberList.focus()
			return false
		}else if (cityObj.value == "" && stateObj.value != "" )
		{
			alert("Please enter a City")
			stateObj.focus()
			return false
		}else if (stateObj.value == "" && cityObj.value != "" )
		{
			alert("Please enter a State")
			stateObj.focus()
			return false
		}
		
		return true		
	}
</script>


