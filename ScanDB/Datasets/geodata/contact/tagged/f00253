/*
Preload images script
*/
var PreImages=new Array();
function PreloadIMG()
{
	for (i=0;i<PreloadIMG.arguments.length;i++)
	{
	PreImages[i]=new Image()
	PreImages[i].src=PreloadIMG.arguments[i]
	}
}
//Highlights an image when it's moused over
function MyimgOn(iname,ipath,iext) {
  if (document.images) {
      document[iname].src = ipath + "On." + iext;
   }
}

//Restores an image when it's moused out
function MyimgOff(iname,ipath,iext) {
  if (document.images) {
      document[iname].src = ipath +  "." + iext;
   }
}

//-->
