imageStay = ""



function imageon(name){

	if (imageStay != name){

        	document[name].src = eval(name + "on.src");

	}

}

function imageoff(name){

	if (imageStay != name){

        	document[name].src = eval(name + ".src");

	}

}



function on(name){

        if (check == true){

	imageon(name);

	}

}

function off(name){

        if (check == true){

	imageoff(name);

	}

}



function stay(name){

	if (check == true){

	document[name].src = eval(name + "stay.src");

	if (imageStay != name){

		if (imageStay != ""){

			document[imageStay].src = eval(imageStay + ".src");

		}

	}

	imageStay = name

	}

}

