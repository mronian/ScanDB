<script langauge="javascript">
var pnavItems=Array('programs','success','recipes','shop','etools','planner');
/*if (typeof(pnavItems) == "undefined") {
    var pnavItems = new Array();
}
if (typeof(pnavSelected) == "undefined") {
     var pnavSelected = "";
}*/
var pnavSelected = 'corporate';
for(i=0;i<pnavItems.length;i++) {
  eval("var "+pnavItems[i]+"_i=new Image();");
  eval("var "+pnavItems[i]+"_a=new Image();");
  eval(pnavItems[i]+"_i.src='/img/nav/"+pnavItems[i]+"_i.gif';");
  eval(pnavItems[i]+"_a.src='/img/nav/"+pnavItems[i]+"_a.gif';");
}

function swap(n,act) {
  if(document.images && pnavSelected != pnavItems[n]) {
    eval("document."+pnavItems[n]+".src="+pnavItems[n]+"_"+act+".src;");
  }
}
function clearBox(obj) {
  obj.value = "";
}
</script>
