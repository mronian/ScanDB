<SCRIPT TYPE="text/JAVASCRIPT">
  function open_window(url){
  mywin=
 window.open(url,"win",'toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=0,screenX=0,screenY=0,top=0,left=0,width=520,height=410');}
 </script>
<script>

var SlideShowSpeed = 7500;
var CrossFadeDuration = 3;

var Picture = new Array(); // don't change this
var Caption = new Array(); // don't change this


Picture[1]  = 'images/quickfact1.jpg ';
Picture[2]  = 'images/quickfact2.jpg ';
Picture[3]  = 'images/quickfact3.jpg ';
Picture[4]  = 'images/quickfact4.jpg ';
Picture[5]  = 'images/quickfact5.jpg ';



Caption[1]  = "DOI protects 1,265 endangered or threatened species in the United States.";
Caption[2]  = "The Bureau of Reclamation supplies the water that grows 60% of America's vegetables.";
Caption[3]  = "DOI manages 507 million acres of surface land, or about one-fifth of the land in the U.S.";
Caption[4]  = "BIA provides education services to 47,671 children at 184 sites.";
Caption[5]  = "68% of oil and gas reserves are managed by the U.S. Department of the Interior.";


var tss;
var iss;
var jss = 1;
var pss = Picture.length-1;

var preLoad = new Array();
for (iss = 1; iss < pss+1; iss++){
preLoad[iss] = new Image();
preLoad[iss].src = Picture[iss];}

function runSlideShow(){
if (document.all){
document.images.PictureBox.style.filter="blendTrans(duration=2)";
document.images.PictureBox.style.filter="blendTrans(duration=CrossFadeDuration)";
document.images.PictureBox.filters.blendTrans.Apply();}
document.images.PictureBox.src = preLoad[jss].src;
if (document.getElementById) document.getElementById("CaptionBox").innerHTML= Caption[jss];
if (document.all) document.images.PictureBox.filters.blendTrans.Play();
jss = jss + 1;
if (jss > (pss)) jss=1;
tss = setTimeout('runSlideShow()', SlideShowSpeed);
}

</script>
</head>


<BODY onload=runSlideShow()>
        

  
