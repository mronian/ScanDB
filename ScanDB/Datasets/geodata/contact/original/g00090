<SCRIPT LANGUAGE="JavaScript">
//CPPE Registration Form

function isNumber(theField)
	{
	oneDecimal = false
	fldValue = theField.value
	inputStr = "" + fldValue
	for (var i = 0; i < inputStr.length; i++)
		{
		var oneChar = inputStr.charAt(i)
			if (oneChar == "." && !oneDecimal)
			{
			oneDecimal = true
			continue
			}
		if (oneChar < "0" || oneChar > "9")
			{
			alert ("This field accepts digits and a decimal point only!")
			theField.value = eval(0)
			theField.focus()
			theField.select()
			return(false)
			break
			}
		}
		return (true)
	}


function MakeArray(n) {
        this.length = n;
        return this;
	}

monthNames = new MakeArray(12);
monthNames[1] = "January";
monthNames[2] = "February";
monthNames[3] = "March";
monthNames[4] = "April";
monthNames[5] = "May";
monthNames[6] = "June";
monthNames[7] = "July";
monthNames[8] = "August";
monthNames[9] = "September";
monthNames[10] = "October";
monthNames[11] = "November";
monthNames[12] = "December";


monthShortNames = new MakeArray(12);
monthNames[1] = "Jan.";
monthNames[2] = "Feb.";
monthNames[3] = "Mar.";
monthNames[4] = "Apr.";
monthNames[5] = "May";
monthNames[6] = "Jun.";
monthNames[7] = "Jul.";
monthNames[8] = "Aug.";
monthNames[9] = "Sep.";
monthNames[10] = "Oct.";
monthNames[11] = "Nov.";
monthNames[12] = "Dec.";


dayNames = new MakeArray(7);
dayNames[1] = "Sunday";
dayNames[2] = "Monday";
dayNames[3] = "Tuesday";
dayNames[4] = "Wednesday";
dayNames[5] = "Thursday";
dayNames[6] = "Friday";
dayNames[7] = "Saturday";


function customDateString(oneDate) {
        var theDay = dayNames[oneDate.getDay() + 1];
        var theMonth = monthNames[oneDate.getMonth() + 1];
        var theYear = oneDate.getFullYear();
        return theDay + ", " + theMonth + " " + oneDate.getDate() + ", " + theYear;
	}


function formDateString(oneDate) {
        var theDay = oneDate.getDay();
        var formMonth = monthShortNames[oneDate.getMonth() + 1];
        var theYear = oneDate.getFullYear();

	}


function setGsorcomp_toGov(which)
	{
		which.checked=1;
	}


function initGSLevel(which)
	{
		which.checked=1;
	}


function clearGovLevels(which)
	{
			for (var j=0; j<9; j++)
			{
				which[j].checked=0;
			}
	}


function clearSubNos(which,wno1,wno2,wno3)
	{
				wno1.checked=0;
				wno2.checked=0;
				wno3.checked=0;
	}


function uncheckOpp(whichOpp)
	{
		whichOpp.checked=1;
	}


//  === Stop hiding script ======================================= -->
</SCRIPT>
 
<script language="JavaScript">
//for survey form 
 var newWindow=null;
 function makeNewWindow(newURL)
  {
  showURL="" + newURL
  newWindow = window.open(showURL,"new_window","toolbar=1,location=1,directories=0,status=1,menubar=0,scrollbars=1,resizable=1,copyhistory=1,width=640,height=300")
  }
 
function clearSubYeses(which,wyes1,wyes2,wyes3)
 {
   for (var j=0; j<3; j++)
   {
    wyes1[j].checked=0;
    wyes2[j].checked=0;
    wyes3[j].checked=0;
   }
 }
 
function clearSubNos(which,wno1,wno2,wno3)
 {
    wno1.checked=0;
    wno2.checked=0;
    wno3.checked=0;
 }
 
function uncheckOpp(whichOpp)
 {
  whichOpp.checked=1;
 }
 
</script>



