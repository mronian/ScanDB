<script language="JavaScript1.2">
<!-- //
// DHTML MENU
function toggle(item,state){
	if (menus && IE4) {
		// toggle menu for IE
		menu = eval(item + "Menu");
		menu.style.display = (menu.style.display == "none" ) ? "" : "none";
		// toggle arrow for IE
		if (state == "on") {
			state = "_on";
		}
		if (menu.style.display == "none") {
				document.images[item].src = eval("arrow_up" + state + ".src");
		} else {
				document.images[item].src = eval("arrow_down" + state + ".src");
		}
	} else if (menus && NS4) {
		// toggle menu for NS
		menu = eval("document." + item + "Menu");
		if (menu.visibility == "hide") {
			menu.visibility = "show";
		}
		else {
			menu.visibility = "hide";
		}
		// toggle arrow for NS
		image = eval("document." + item + "Toggle.document.images['" + item + "']");
		if (menu.visibility == "hide") {
			if (state == "on") {
				image.src = arrow_up_on.src;
			} else {
				image.src = arrow_up.src;
			}
		} else {
			if (state == "on") {
				image.src = arrow_down_on.src;
			} else {
				image.src = arrow_down.src;
			}
		}
		display();
	} else {
		// does not support dhtml
		top.location.href = "/" + item + "/";
	}
}

// STYLE SHEET
document.write("<style type='text/css'>");
if (menus && NS4) {
	document.write(".navmenu {position:absolute; visibility:visible; left:7px;}");
	document.write(".subnavmenu {position:absolute; visibility:hidden; left:7px;}");
	document.write(".subnavmenu_on {position:absolute; visibility:visible; left:7px;}");
	document.write(".nomenu {position:absolute; visibility:visible; left:7px;}")
} else if (NS6) {
	document.write(".subnavmenu {position:absolute; visibility:hidden}")
} else if (menus && IE4) {
	document.write(".subnavmenu {display: }")
}
document.write("<" + "/style>");

// NETSCAPE DISPLAY FUNCTIONS
function getindex(m) {
	 if (menus && NS4) {
		ind = null;
		for (i=0; i<document.layers.length; i++) {
			menu = document.layers[i];
			if (menu.id == m) {
				ind = i;
				break;
			}
		}
		return ind;
	}
}
function display() {
	 if (menus && NS4) {
		nextmenu = document.layers[firstindex].pageY + document.layers[firstindex].document.height;
		for (i=firstindex+1; i<document.layers.length; i++) {
			menu = document.layers[i];
			if (menu.visibility != "hide") {
				menu.pageY = nextmenu;
				nextmenu += menu.document.height;
			}
		}
	}
}

// INIT FUNCTION
function initmenus(){
	if (menus && NS4) {
		firstm = "homeToggle";
		firstindex = getindex(firstm);
		display();
	} else if (IE4) {
		divs = document.all.tags("div");
		for (i=0; i<divs.length; i++) {
			menu = divs(i);
			if (menu.className == "subnavmenu") {menu.style.display = "none"};
		}
	}
}

// -->
</script>

<script language="JavaScript">
<!-- //

// TEMPORARY DATE SCRIPT (WILL BE SERVER-SIDE)
var months = new Array('January','February','March','April','May','June','July','August','September','October','November','December');
var today = new Date();
var monthname = months[today.getMonth()];
var day = today.getDate();
var year = thisyear(today);
function thisyear(t) {
	x = t.getYear();
	var y = x % 100;
	y += (y < 38) ? 2000 : 1900;
	return y;
}

// -->
</script>

<script language="JavaScript1.2">
function mailhide(Author_Name,Email_Account,Domain_Name,Suffix)
{
	var defAuthor="Brookings"
	var defAccount="webmaster";
	var defDomain="brookings";
	var defSuffix="edu"
	
	if(!Author_Name) Author_Name=defAuthor;
	if(!Email_Account) Email_Account=defName;
	if(!Domain_Name) Domain_Name=defDomain;
	if(!Suffix) Suffix=defSuffix;
	
	document.write('<a href=\"mailto:'+Email_Account+'@'+Domain_Name+'.'+Suffix+'\">');
	document.write(Author_Name+'</a>');
}
function mailhide2(Email_Account,Domain_Name,Suffix,Title)
{
	
	var defAccount="webmaster";
	var defDomain="brookings";
	var defSuffix="edu"
	var defTitle = " ";
	
	
	if(!Email_Account) Email_Account=defName;
	if(!Domain_Name) Domain_Name=defDomain;
	if(!Suffix) Suffix=defSuffix;
	if(!Title) Title=defTitle;
	
	document.write('<a href=\"mailto:'+Email_Account+'@'+Domain_Name+'.'+Suffix+'?subject='+Title+'\">');
	
}

function makenewwindow(newURL) {
     var newwindow=null;
     showURL="" + newURL;
     newWindow = window.open(showURL,"new_window","toolbar=1,location=1,directories=0,status=1,menubar=0,scrollbars=1,resizable=1,copyhistory=1,width=600,height=300");
     } 



function makeNewWindow(newURL) {
     var newwindow=null;
     showURL="" + newURL;
     newWindow = window.open(showURL,"new_window","toolbar=1,location=1,directories=0,status=1,menubar=0,scrollbars=1,resizable=1,copyhistory=1,width=600,height=300");
     } 

</script>

