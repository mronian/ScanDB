<script language="javascript">
  // THIS METHOD IS USED BY HITBOX-WEB-REPORTING ... 
  var searchTerm = ""; 
  var searchItmCnt = ""; 
  function hbxStrip(a) { 
     a = a.split("|").join(""); 
     a = a.split("&").join("_"); 
     a = a.split("'").join(""); 
     a = a.split("#").join(""); 
     a = a.split("$").join(""); 
     a = a.split("%").join(""); 
     a = a.split("^").join(""); 
     a = a.split("*").join(""); 
     a = a.split(":").join(""); 
     a = a.split("!").join(""); 
     a = a.split(">").join(""); 
     a = a.split("<").join(""); 
     a = a.split("~").join(""); 
     a = a.split(";").join(""); 
     a = a.split(" ").join("+"); 
     return a; 
  }
</script>
<script language="javascript">
function createBookmark(){
	var params = read_querystring();

	var url = "http://"+location.host+"/presenter.jhtml?"
	var pos= url.indexOf("?identifier=");
	if (pos<0){
		url = url + "identifier="+params["identifier"];
	}
	var pos= url.indexOf("&dynamicPage=");
	if (pos<0){
		url = url + "&dynamicPage=/quizzes/quizResults.jhtml";
	}
	var pos= url.indexOf("&dynamicPageNumber=");
	if (pos<0){
		url = url + "&dynamicPageNumber="+params["dynamicPageNumber"];
	}
	var pos= url.indexOf("&stashId=");
	if (pos<0){
		url = url + "&stashId="+params["stashId"];
	}
	pos= url.indexOf("&quizName=");
	if (pos<0){
		url = url + "&quizName="+params["quizName"];
	}

	if (window.sidebar) { 
		window.sidebar.addPanel("AHA - Learn and Live Quiz Results", url,""); 
	}
	else{		
		window.external.AddFavorite(url,"AHA - Learn and Live Quiz Results");
	}
//	window.location.reload();
}

function read_querystring()
   {
   var a_out = new Object();
   var s_loc = String(location.href);

   if(s_loc.indexOf('?')>0)
     {
     var p;
     var s_query = s_loc.substr(s_loc.indexOf('?')+1);
     var a_query = s_query ? s_query.split('&') : new Array();

     for(var i=0; i<a_query.length; i++)
       {
       p = a_query[i].split('=');
       a_out[p[0]] = p[1].replace(/\+/g, ' ');
       }
     }
   return a_out;
   }
</script>

 
 
 
 
 
