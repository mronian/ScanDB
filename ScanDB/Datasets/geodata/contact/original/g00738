<SCRIPT>
function fixdomain(){
  var strText = new String() ;
  var newstr = new String();
  wherearewe = document.location;
  strText = "" + wherearewe;  

var re = new RegExp ('county.maui.net');
var newstr = strText.replace(re, 'www.co.maui.hi.us')

var re2 = new RegExp ('mauico.maui.net');
var newstr = strText.replace(re2, 'www.co.maui.hi.us')

if(newstr != strText){
  window.location.replace(newstr);
}

}
  
</SCRIPT>
