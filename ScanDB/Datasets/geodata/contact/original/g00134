 function doChangeImages(pStr,pBool)
 {
  tmpImg = eval('document.images["' + pStr + '"]');
  tmpStr = pStr;
  tmpImg.src = (pBool) ? tmpStr + "_on.gif" : tmpStr + "_off.gif";
 }
</script>
