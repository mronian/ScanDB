function myString(title,body) {
	var s = '';
	if (title && body) {
		s = "&nbsp;<b>" + title + "</b>&nbsp;<BR>" + body;
	} else if (title) {
		s = "&nbsp;<b>" + title + "</b>&nbsp;";
	} else if (body) {
		s = body;
	}
	return s;
}

function doHigh(link,ishigh) {
	var s;
	if (ishigh) {
		s = link + " onfontcolor=" + mouseOnFontColor + ";onbackcolor=" + mouseOnBgColor + ";offfontcolor=" + mouseOnFontColor + ";offbackcolor=" + mouseOnBgColor + ";"
	} else {
		s = link;
	}
	return s;
}

