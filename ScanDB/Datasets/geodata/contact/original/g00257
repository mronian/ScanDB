<script Language="JavaScript"><!--
function makeSure(theForm)
{
  var info = 0;
  if ((theForm.more_info2.checked) || (theForm.more_info3.checked) || (theForm.more_info4.checked) || (theForm.more_info5.checked))
  { info = 1; 
  }
  if (theForm.first_name.value == "")
  {
    alert("Please enter a value for the \"First Name\" field.");
    theForm.first_name.focus();
    return (false);
  }

  if (theForm.last_name.value == "")
  {
    alert("Please enter a value for the \"Last Name\" field.");
    theForm.last_name.focus();
    return (false);
  }

  if (theForm.email.value == "")
  {
    alert("Please enter a value for the \"Email\" field.");
    theForm.email.focus();
    return (false);
  }

  if (theForm.zip.value == "")
  {
    alert("Please enter a value for the \"Zipcode\" field.");
    theForm.zip.focus();
    return (false);
  }
  
  if ((theForm.work_phone.value == "") && (info == 1))
  {
    alert("When \"More Information\" is selected, you must enter a value for the \"Work Phone\" field.");
    theForm.work_phone.focus();
    return (false);
  }
  
  if ((theForm.address.value == "") && (info == 1))
  {
    alert("When \"More Information\" is selected, you must enter a value for the \"Address\" field.");
    theForm.address.focus();
    return (false);
  }
  
  if ((theForm.city.value == "") && (info == 1))
  {
    alert("When \"More Information\" is selected, you must enter a value for the \"City\" field.");
    theForm.city.focus();
    return (false);
  }
  
  if ((theForm.state.value == "") && (info == 1))
  {
    alert("When \"More Information\" is selected, you must enter a value for the \"State\" field.");
    theForm.state.focus();
    return (false);
  }
  
  if ((theForm.country.value == "") && (info == 1))
  {
    alert("When \"More Information\" is selected, you must enter a value for the \"Country\" field.");
    theForm.country.focus();
    return (false);
  }
    return (true);
}
//--></script>

