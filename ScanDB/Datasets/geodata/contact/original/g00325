<SCRIPT language="JavaScript" type="text/javascript">
function GoToCareCenter()
{
	if(document.CCForm.CareCenter.selectedIndex !=0 && document.CCForm.CareCenter.selectedIndex !=1 && document.CCForm.CareCenter.selectedIndex !=3)
	{
		window.location = document.CCForm.CareCenter.options[document.CCForm.CareCenter.selectedIndex].value;
	}
}	

function GoToDisease()
{
	if(document.DiseaseForm.Disease.selectedIndex !=0 && document.DiseaseForm.Disease.selectedIndex !=1)
	{
		window.location = document.DiseaseForm.Disease.options[document.DiseaseForm.Disease.selectedIndex].value;
	}
}	
</script>


	


	


