<script language="JavaScript">
function goodMail(m) {
	var goodEmail = m.match(/\b(^(\S+@).+((\.com)|(\.net)|(\.edu)|(\.mil)|(\.gov)|(\.org)|(\..{2,2}))$)\b/gi)
	var passMess1 = ""
	if (!goodEmail) {
		passMess1 = "\n - Your email address must be properly formatted."
		return passMess1
	}
}

function sendForm() {
	var temp
	var rootMsg = "Please correct the following problems:"
	var theMsg = "Please correct the following problems:"
	with (document.theForm) {
		if (Country.value == "") {
			theMsg = theMsg + "\n - Please select a country."
		} else {
			if (Country.value == "US" && State.value == "") {
				theMsg = theMsg + "\n - Please select a state."
			}
		}
		if (Email.value != "") {
			var msg = goodMail(Email.value)
			if (msg != "") theMsg = theMsg + msg
		}
		if (Message.value == "") {
			theMsg = theMsg + "\n - Please input your comment."
		}
		if (theMsg == rootMsg || theMsg == rootMsg + undefined) {
			document.theForm.submit()
		} else {
			alert(theMsg)
			return false
		}
	}
}
</script>
</head>
