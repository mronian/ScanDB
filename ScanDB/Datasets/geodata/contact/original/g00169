<script language="JavaScript">
function cleankeywords1() {
	with (self.document.jobsearch_left) {
		if (JobKeywords_Keyword.value == "enter keywords") {
			JobKeywords_Keyword.value = "";
			return true;
		}

		var sValue = getCleanKeywords(JobKeywords_Keyword.value);
        JobKeywords_Keyword.value = sValue;
        return true;
	}
}
 </script>


