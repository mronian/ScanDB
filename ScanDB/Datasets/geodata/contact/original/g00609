function turnOn(imageName) {
  if (document.images) {
	var imageIndex = parseInt(imageName.substring(5));
    document[imageName].src = imageon[imageIndex].src;
  }
}
function turnOff(imageName) {
  if (document.images) {
	var imageIndex = parseInt(imageName.substring(5));
    document[imageName].src = imageoff[imageIndex].src;
  }
}


