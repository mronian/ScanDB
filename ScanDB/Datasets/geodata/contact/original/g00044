<!--
var zedo_show = zedoExitPopFlag;
var zedo_dom = ".statesman.com"; // Change this to appropriate domain

function zedo_lifetime() {
    var zedo_now = new Date();
    var zedo_expire = new Date();

    zedo_expire.setUTCHours(5);
    zedo_expire.setUTCMinutes(0);
    zedo_expire.setUTCSeconds(0);

    var zedo_life = zedo_expire.getTime() - zedo_now.getTime();

    if ( zedo_life < 0 ) {
        zedo_expire.setUTCDate(zedo_expire.getUTCDate() + 1);
    }
    return zedo_expire.toUTCString();
}

function reset() {
    zedo_show=false;
    if (this.oldonclick != null ) {
        this.oldonclick();
    }
    return true;
}

function update_links() {
    for( var i=0;i <document.links.length;++i ) {
        document.links[i].oldonclick = document.links[i].onclick;
        document.links[i].onclick = reset;
        if ( window.oldonload != null ) {
            window.oldonload();
        }
    }
}

function show_pop() {
    if (zedo_show && document.cookie.match(/ZEDO162000260/) == null) {
        var win = window.open("http://c1.zedo.com/jsc/c1/ff2.html?n=162;c=260/97/1;s=223;d=16;w=720;h=300;t=CXnt Advertisement - Undertone Networks","_blank","toolbar=no,menubar=no,resizable=yes,location=no,scrollbars=no,width=720,height=300");
        if ( win != null ) {
            win.blur();
        }
        document.cookie="ZEDO162000260=1;expires="+zedo_lifetime()+ ";domain=" + zedo_dom  + ";path=/";
    }

}

if (document.all && navigator.cookieEnabled && (document.domain.indexOf(zedo_dom) != -1) && document.cookie.match(/ZEDO162000260/) == null) {
    window.oldonload = window.onload;
    window.onload = update_links;
    window.onunload = show_pop;
}
//-->
</script>
