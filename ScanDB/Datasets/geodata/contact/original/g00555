	<script language="JavaScript">
	var clickFlag = "false";
	function clearValue(){
		if(clickFlag=="false"){
			document.nlSignup.email.value = "";
			clickFlag = "true";
		}
	}
	</script>
	<script language="Javascript">
function submitValue(){
validRegExp = /^[^@]+@[^@]+.[a-z]{2,}$/i;
strEmail = document.nlSignup.email.value;
	if (strEmail.search(validRegExp) == -1) {
		alert(" A valid e-mail address is required.\r\Please make sure you enter your full email address");
		return false;
	} else {
		document.forms['nlSignup'].submit();
	}
}
</script>
    <form name="nlSignup" method="post" action="/common/nl_confirm.asp" onsubmit="return checkEmail(nlSignup)">
