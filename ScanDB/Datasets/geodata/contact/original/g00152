<script language="JavaScript" type="text/javascript">
function submit_form(){
	document.Contact_Look_Up.submit()
}
function SetFocus(){
				document.Contact_Look_Up.SearchStr.focus();
			}
// -------------------------------------FIXES THE PUNCTUATION IN THE FORM
 function FixTextField(fixform){
	 var temp, len, loc
	 var newString = ""
	 loc = -1
	 len = fixform.value.length
	 for (var i=0; i<len;i++) {
	   temp = "" + fixform.value.substring(i,i+1)
	 	// check for "
	      if (fixform.value.substring(i,i+1) == '"') {
		      newString = newString
	      } else
	    // check for *
	      if (fixform.value.substring(i,i+1) == "*") {
		      newString = newString + "%"
	      } else
		   // check for '
	      if (fixform.value.substring(i,i+1) == "'") {
		      newString = newString + "’"
	      }else
		  {
	      newString = newString + fixform.value.substring(i,i+1)
	   		}
	 	}
		fixform.value = newString
}
function OpenWin(location) {
		dialWindow = window.open(location,'','width=700,height=400,toolbar=no,scrollbars=yes,location=no,resizable=yes')
	}
</script>
