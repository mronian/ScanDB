<script language="JavaScript" type="text/javascript">
<!--
REQS = new Array();

REQS[0] = ["from","Text"];
REQS[1] = ["firstname","Text"];
REQS[2] = ["lastname","Text"];
REQS[3] = ["address","Text"];
REQS[4] = ["city","Text"];
REQS[5] = ["state","Select"];
REQS[6] = ["zip","Text"];
REQS[7] = ["phone","Text"];
REQS[8] = ["to","Select"];
					
function check_form(f){
	isCC = 0;
	
	for(t = 0; t < REQS.length; t++){
		if(REQS[t][0] == "from"){
			if(!checke(f.from.value)){
				alert("Error: \nPlease enter a valid e-mail address."); 
				f.from.focus();
				if(document.all || document.getElementById){
					f.from.style.background = "#FFFF00";
				}
				return (false); 
			}
		} else {
			if(REQS[t][1] == "Text" || REQS[t][1] == "textarea" || REQS[t][1] == "File"){
				if(REQS[t][0] != "ccnum" && REQS[t][0] != "ccname"){
					if(f[REQS[t][0]].value.length < 1){
						alert("Error: \nPlease check the field with focus.");
						f[REQS[t][0]].focus();
						if(document.all || document.getElementById){
							f[REQS[t][0]].style.background = "#FFFF00";
						}
						return (false);
					}
				}
			}
			if(REQS[t][1] == "Select" && REQS[t][0] != "ccexp" && REQS[t][0] != "ccexp_y"){
				if(f[REQS[t][0]].selectedIndex < 1){
					alert("Error: \nPlease check the field with focus.");
					f[REQS[t][0]].focus();
					if(document.all || document.getElementById){
						f[REQS[t][0]].style.background = "#FFFF00";
					}
					return (false);
				}
			}
			if(REQS[t][1] == "Checkbox"){
				if(!f[REQS[t][0]].checked){
					alert("Error: \nPlease check the field with focus.");
					f[REQS[t][0]].focus();
					if(document.all || document.getElementById){
						f[REQS[t][0]].style.background = "#FFFF00";
					}
					return (false);
				}
			}
			if(REQS[t][1] == "Radio"){
				isRad = 0;
				for(r = 0; r < f[REQS[t][0]].length; r++){
					if(f[REQS[t][0]][r].checked){
						isRad++;
					}
				}
				if(!isRad){
					alert("Error: \nPlease check the field with focus.");
					f[REQS[t][0]][0].focus();
					if(document.all || document.getElementById){
						f[REQS[t][0]][0].style.background = "#FFFF00";
					}
					return (false);
				}
			}
			
		}
	}
	if(f.from){
		if(f.from.value.length > 0){
			if(!checke(f.from.value)){
				alert("Error: \nPlease enter a valid e-mail address."); 
				f.from.focus();
				if(document.all || document.getElementById){
					f.from.style.background = "#FFFF00";
				}
				return (false); 
			}
		}
	}
}
// -->
</script>


