// Print our 'Welcome'
document.write("Welcome, <b>");

// Process our cookie for the dashboard
var insiteCookie = "tacoma_user_auth";
var myCookie = "tacoma_dat";
var cookieString = readCookie(myCookie);
var myKey, myVal = "";

if (cookieString) {
	myKey = "first_name"
	myVal = parseCookie(cookieString, myKey);
	if (myVal) {
		document.write(myVal);
	}
} else {
	document.write("Guest"); 
}
document.write("</b><br>");

if (GetCookie(insiteCookie) && !GetCookie(insiteCookie).match("threshold")) { 
	document.write("<table width='100%' cellpadding='0' cellspacing='2'><tr><td scope='col' class='uinfo'>&#8226;&nbsp;<a href='/register-bin/trusted_interface.pl.cgi?mode=edit'>Edit your account</a><br>&#8226;&nbsp;<a href='/register-bin/trusted_interface.pl.cgi?mode=change_password'>Change password</a><br>&#8226;&nbsp;<a href='/register-bin/trusted_interface.pl.cgi?mode=logout'>Log out </a></td></tr></table>");
