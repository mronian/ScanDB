			function logOut() {

				confBox = confirm("Logging out will delete any items you may have added to your shopping cart. Are you sure you want to continue and log out?")

				if (confBox == true) { location = "/NASApp/registration/do/Logoff" }

			}

			var cookieTest = document.cookie

			if (cookieTest.indexOf('SMSESSION') >= 0 && cookieTest.indexOf('LOGGEDOFF') == -1) {

				document.write('<a href="javascript:logOut()"><img src="/img/topnav/button_logout.gif" border=0 width=69 height=19 hspace=4></a>');

			} else if (hasAribaCookie()) {
			
							document.write('');
						
