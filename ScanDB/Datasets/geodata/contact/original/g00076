<script language="javascript">

autoRedirect();

function validateSearch(form) {	
	var hostName = window.location.hostname
	hostSet = new Array( ["www.nextel.com","nextelonline.nextel.com"],["mulan.nextel.com","nextelonline.nextel.com"],["www-test.nextel.com","noltest.nextel.com"],["localhost","noltest.nextel.com"] )
	for (x=0;x<hostSet.length;x++) {
		if (hostName == hostSet[x][0]) {
			hostName = hostSet[x][1]
			break
		}
	}
	document.verity_search.action="http://" + hostName + "/NASApp/search/search.jsp";
	document.verity_search.submit();
}
</script>



