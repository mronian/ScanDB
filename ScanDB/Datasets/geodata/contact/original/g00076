function homeLink() {
	if (window.pubArray) { sourceRedirect(1) }
	else { location = "/index.shtml" }
}

function open_popup1 (file,height,width) {
	yheight = height; xwidth = width; url = file;
	newWindow = eval('window.open(url,"new_window","height=' + yheight + ',width=' + xwidth + ',scrollbars=yes,menubar=yes,directories=no,location=no,status=yes,resizable=yes")')
	newWindow.focus();
}

function hasAribaCookie() {
	cookieContentsAriba = document.cookie
	cookieIndexAriba = cookieContentsAriba.indexOf('AribaEnabledAisle')
	if (cookieIndexAriba >= 0) { 
		return true
	} else {
		return false
	}
}

--></script>
