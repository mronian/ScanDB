owMenu(inName + "L");			}		}				function rollOff(inName)		{			if(document.images)			{				document.images[inName].src = eval(inName + "1.src");				myTimer = setTimeout("hideAllMenus()",1500);				enteredMenu = 1;			}		}		function setTimer()		{			myTimer = setTimeout("hideAllMenus()",1500);		}		function clearTimer()		{			clearTimeout(myTimer);		}		function showMenu(theMenu)		{			document.getElementById(theMenu).style.top = (mPosY - 10) + 'px';			document.getElementById(theMenu).style.visibility = 'visible';		}		function hideMenu(theMenu)		{			document.getElementById(theMenu).style.visibility = 'hidden';		}		function hideAllMenus()		{			document.getElementById("aboutus_leftL").style.visibility = 'hidden';			document.getElementById("boardofdirectors_leftL").style.visibility = 'hidden';			document.getElementById("bridge_leftL").style.visibility = 'hidden';			document.getElementById("ferry_leftL").style.visibility = 'hidden';			document.getElementById("transit_leftL").style.visibility = 'hidden';			document.getElementById("newsevents_leftL").style.visibility = 'hidden';		}		function hideOtherMenus(menuName)		{			if(menuName != "aboutus_leftL") document.getElementById("aboutus_leftL").style.visibility = 'hidden';			if(menuName != "boardofdirectors_leftL") document.getElementById("boardofdirectors_leftL").style.visibility = 'hidden';			if(menuName != "bridge_leftL") document.getElementById("bridge_leftL").style.visibility = 'hidden';			if(menuName != "ferry_leftL") document.getElementById("ferry_leftL").style.visibility = 'hidden';			if(menuName != "transit_leftL") document.getElementById("transit_leftL").style.visibility = 'hidden';			if(menuName != "newsevents_leftL") document.getElementById("newsevents_leftL").style.visibility = 'hidden';		}			//-->	</script>

